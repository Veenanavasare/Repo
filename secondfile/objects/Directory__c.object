    <validationRules>
        <fullName>Check_user_type</fullName>
        <active>true</active>
        <description>This validation rule is used check which all EBM users will be able to edit fields on directory record</description>
        <errorConditionFormula>AND( 
NOT(ISNEW()),
NOT($User.Directory_Manager__c ),
$User.Username &lt;&gt; Legal_Reviewer__r.Username,
$User.Username &lt;&gt;  Marketing_Consultant__r.Username,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;SI_API User&quot;,
RecordType.DeveloperName  &lt;&gt; &quot;Enablement_Resource&quot;
)</errorConditionFormula>
        <errorMessage>You are not authorized to modify Directory record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Description_Authenticated_field_length</fullName>
        <active>true</active>
        <description>The Length of the Description  Authenticated field should not be more than 100.</description>
        <errorConditionFormula>(LEN( Description_Authenticated__c )  &gt; 100)</errorConditionFormula>
        <errorMessage>Length of the Description  Authenticated field should not be more than 100 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_statuses_available_to_EBM_Users</fullName>
        <active>true</active>
        <description>EBMs can only set Pending Legal Approval, Review Requested, Published</description>
        <errorConditionFormula>AND(
ISCHANGED(Status__c),
 NOT(Directory_Check_for_Archived_Status__c),
OR(ISPICKVAL(Status__c ,&quot;Pending Approval&quot;),
ISPICKVAL(Status__c ,&quot;Archived&quot;),
ISPICKVAL(Status__c ,&quot;Draft&quot;),
ISPICKVAL(Status__c , &quot;Deleted&quot;)),
$Profile.Name &lt;&gt; &quot;SI_API User&quot;,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.DeveloperName &lt;&gt; &quot;Enablement_Resource&quot; 
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>This status may only be selected by the SP in the PE Portal. Please set a valid status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_set_to_published</fullName>
        <active>true</active>
        <description>If the Status is Published the following fields Company Display Name,Tagline,Summary,Description,Solutions,City,State,Country,Website,Primary Email,Primary Phone Number,Logo Link,Full Size Logo Link,
Demonstration cannot be null:</description>
        <errorConditionFormula>AND
(
RecordType.Name   =   &apos;Enablement Resource&apos;,
ISPICKVAL( Status__c ,&quot;Published&quot;),
OR(
ISBLANK( Company_Display_Name__c), 
ISBLANK(Summary__c) ,
ISBLANK( Tagline__c ),
ISBLANK(  City__c  ),
ISBLANK(  State__c  ),
ISBLANK(  Country__c  ),
ISBLANK(  Website__c  ),
ISBLANK(  Primary_Email_Address__c  ),
ISBLANK(  Phone_Number__c  ),
ISBLANK(  Logo_Link__c  ),
ISBLANK(  Full_Size_Logo__c  ),
OR(ISBLANK( Demo_Video__c &amp; Image_Link__c ))
)
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>If the Status is Published the following fields Company Display Name,Tagline,Summary,Description,Solutions,City,State,Country,Website,Primary Email,Primary Phone Number,Logo Link,Full Size Logo Link,
Demonstration cannot be null:</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Summary_Authenticated_field_length</fullName>
        <active>true</active>
        <description>The Length of the Summary-Authenticated field should not be more than 50 characters</description>
        <errorConditionFormula>(LEN( Summary_Authenticated__c ) &gt; 50)</errorConditionFormula>
        <errorMessage>The Length of the Summary  Authenticated field should not be more than 50 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Summary_Field_Length</fullName>
        <active>true</active>
        <description>The Length of the summary field should not be greater than 250 characters.</description>
        <errorConditionFormula>(LEN( Summary__c )   &gt;   250)</errorConditionFormula>
        <errorMessage>Length of the Summary field should not be more than 250 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_On_Feedback_Fields</fullName>
        <active>true</active>
        <description>Validation on feedback fields on Status &quot;Review Requested &quot;.</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Status__c ,&quot;Review Requested&quot;),
ISBLANK(Provider_Details_Feedback__c),
 ISBLANK( Overview_Tab_Feedback__c ),
  ISBLANK( Details_Tab_Feedback__c ),
  ISBLANK( Connect_Tab_Feedback__c ),
  ISBLANK( Additional_Information_Feedback__c)
)</errorConditionFormula>
        <errorMessage>Feedback should be provided if Status is set to &quot;Review Requested&quot;.</errorMessage>
    </validationRules>
