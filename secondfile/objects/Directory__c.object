        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)}

var opty=sforce.connection.query(&quot;Select Id ,Marketing_Consultant__c,Marketing_Consultant_Approved__c,Legal_Reviewer__c,Legal_Reviewer_Approved__c,Legal_Review_Required__c FROM Directory__c WHERE Id=&apos;{!Directory__c.Id}&apos; &quot;);

var request=opty.getArray(&quot;records&quot;);



if(request[0].Marketing_Consultant_Approved__c==&apos;false&apos; &amp;&amp; request[0].Marketing_Consultant__c!=null){
if((request[0].Marketing_Consultant__c).toString().substring(0,15) ==&apos;{!$User.Id}&apos;){

window.location.href=(&apos;/apex/ProviderDirectoryApproval?Flag=A&amp;id=&apos;+&quot;{!Directory__c.Id}&quot;);

}
else{
alert(&apos;You are Not Authorized Approver&apos;);
}
}

else if(request[0].Marketing_Consultant_Approved__c &amp;&amp; request[0].Legal_Reviewer__c!=null){
if((request[0].Legal_Reviewer__c).toString().substring(0,15) ==&apos;{!$User.Id}&apos;){

window.location.href=(&apos;/apex/ProviderDirectoryApproval?Flag=A&amp;id=&apos;+&quot;{!Directory__c.Id}&quot;);

}
else{
alert(&apos;You are Not Authorized Approver&apos;);
}
}
else{
alert(&apos;You are Not Authorized Approver&apos;);
}</url>
