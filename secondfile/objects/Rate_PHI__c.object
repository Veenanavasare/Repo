    <webLinks>
        <fullName>Edit_Rates</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Edit Rates</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)};
var ratePhi= new sforce.SObject(&quot;Rate_PHI__c&quot;);
ratePhi.id = &quot;{!Rate_PHI__c.Id}&quot;;
var rates= sforce.connection.query(&quot;SELECT Id,Name,Billing_Element__c,PBI_Number__c FROM Rates__c WHERE Rate_PHI__c=&apos;{!Rate_PHI__c.Id}&apos;&quot;);
var ratesRecords= rates.getArray(&quot;records&quot;);
var valuesnotFound = false;
for (var i=0 ; i&lt;ratesRecords.length; i++)
{
var rate =ratesRecords[i];
if((rate.Billing_Element__c==null || rate.Billing_Element__c==&apos;&apos; )||(rate.PBI_Number__c==null || rate.PBI_Number__c==&apos;&apos;)){
valuesnotFound = true;
break;
}
}
if(valuesnotFound ==true){
alert(&apos;There is no Billing Element or PBI Number present on Rates.&apos;);
}
else{
window.open(&apos;/apex/RateTierDeterminantDisplay?id={!Rate_PHI__c.Id}&apos;);
}</url>
