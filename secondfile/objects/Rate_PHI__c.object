        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)};
var ratePhi= new sforce.SObject(&quot;Rate_PHI__c&quot;);
ratePhi.id = &quot;{!Rate_PHI__c.Id}&quot;;  
var PHIStatus=&apos;{!Rate_PHI__c.Discount_Plan_Status__c}&apos;; 
var ratePlanName=&apos;{!Rate_PHI__c.Rate_Plan_Name__c}&apos;;

if(ratePlanName == &apos;EBM Competitive Feature Discount Plan&apos;)
{
window.open(&apos;/apex/API_CreateDiscountPlan?phiId={!Rate_PHI__c.Id}&apos;,&apos;CreateDiscountPlan&apos;,&quot;height=1500,width=1500,scrollbars=yes&quot;);
}
else
{
alert(&apos;This Rate Plan is already discounted.You can only create discount for competitive feature type.&apos;);
}</url>
