    <webLinks>
        <fullName>Retrigger_Create_Discount_Plan</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Retrigger Create Discount Plan</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}; 
var ratePhi= new sforce.SObject(&quot;Rate_PHI__c&quot;);
ratePhi.id = &quot;{!Rate_PHI__c.Id}&quot;;
var PHIStatus=&apos;{!Rate_PHI__c.Discount_Plan_Status__c}&apos;; 
var ratePlanName=&apos;{!Rate_PHI__c.Rate_Plan_Name__c}&apos;; 
var ratePbiObject= new sforce.SObject(&quot;Rate_PBI__c&quot;);
var ratePBI=sforce.connection.query(&quot;SELECT ID,Discounted_Already__c,Old_PBI_Id__c,Rate_PHI__c FROM Rate_PBI__c WHERE Rate_PHI__c=&apos;{!Rate_PHI__c.Id}&apos;&quot;);
//var oldratePbi = &apos;{!Rate_PBI__c.Old_PBI_Id__c}&apos;; 
var getOldRateObi = sforce.connection.query(&quot;Select id,Discounted_Already__c,Old_PBI_Id__c from Rate_PBI__c WHERE id = &apos;{!Rate_PBI__c.Old_PBI_Id__c}&apos;&quot;);
var childPBI=ratePBI.getArray(&quot;records&quot;); 

if(getOldRateObi.Discounted_Already__c == true){
alert(&apos;This Discount Plan is already discounted.&apos;);
}
if(ratePlanName != &apos;EBM Deal Competitive Feature Discount Plan&apos; &amp;&amp; PHIStatus == &apos;Create Discount Plan Failed&apos;)
{
ratePhi.Discount_Plan_Status__c =&apos;Create Discount Plan In Progress&apos;;
sforce.connection.update([ratePhi]);
//childPBI[0].Discounted_Already__c=true; 
sforce.connection.update([childPBI]);
window.location.href = window.location.href; 
}
else 
{
alert(&apos;To Retrigger create discount plan, the Discount plan status must be failed and record should not be of Competitive Discount Type.&apos;);
}</url>
    </webLinks>
