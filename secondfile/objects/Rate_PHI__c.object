        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)};
var profName= &apos;{!$Profile.Name}&apos;;
if(profName.trim()== &apos;System Administrator&apos; || profName.trim()== &apos;SI_EBM Admin&apos; || profName.trim()== &apos;SI_EBM_DEV_OPS&apos;)
{
