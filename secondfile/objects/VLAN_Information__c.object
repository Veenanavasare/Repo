    <validationRules>
        <fullName>Lookup_to_CVLAN_cannot_be_updated_onVLAN</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED( Circuit_and_VLAN_Details__c )</errorConditionFormula>
        <errorMessage>The reference to Circuit and VLAN Details cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RDS_Edit_Expired</fullName>
        <active>true</active>
        <description>RDS Edit Expired</description>
        <errorConditionFormula>AND($Profile.Name &lt;&gt;&quot;SI_API User&quot; ,
(OR(If(Circuit_and_VLAN_Details__r.Premise__r.DealHash__r.Edit_Expired_Date__c&lt;NOW(),True,False),ISPICKVAL( Circuit_and_VLAN_Details__r.Premise__r.DealHash__r.RDS_object_status__c ,&quot;Review completed&quot;))),
		$Profile.Name &lt;&gt;&quot;SI_API User&quot;,
                $Profile.Name&lt;&gt;&quot;ATT Partner Community User&quot;, 
		$Profile.Name &lt;&gt;&quot;System Administrator&quot;,
$Profile.Name &lt;&gt;&quot;SI_EBM_DEV_OPS&quot;,
$Profile.Name &lt;&gt;&quot;SI_EBM Admin&quot;)</errorConditionFormula>
        <errorMessage>Edit Time has elapsed . Please contact EBM Admin .</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Record_Lock_after_Deal_Hash_creation_VLN</fullName>
        <active>true</active>
        <errorConditionFormula>Circuit_and_VLAN_Details__r.Premise__r.DealHash__r.IsDealHashCreationSuccess__c = true &amp;&amp; $Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; $Profile.Name&lt;&gt;&quot;ATT Partner Community User&quot; &amp;&amp; $Profile.Name &lt;&gt; &apos;SI_API User&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;SI_EBM_DEV_OPS&apos; &amp;&amp; NOT( ISNEW())</errorConditionFormula>
        <errorMessage>Record cannot be updated after Deal Hash has been created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Creation_for_Non_MACD_D_VLAN</fullName>
        <active>true</active>
        <description>This validation rule will prevent creation of records for SI EBM Admin and EBM DEV OPs profile when the record is not of MACD Disconnect type</description>
        <errorConditionFormula>AND(MACD_Type__c &lt;&gt; &apos;D&apos;, MACD_Type__c &lt;&gt; &apos;C&apos;, 
OR($Profile.Name = &apos;SI_EBM_DEV_OPS&apos;,$Profile.Name = &apos;SI_EBM Admin&apos;), 
ISNEW() )</errorConditionFormula>
        <errorMessage>You do not have permission to create a new RDS-VLAN Information record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Updates_for_DevOps_Vlan</fullName>
        <active>false</active>
        <description>EBM DEV Ops Profile users should not be able to update following fields under RDS-VLAN Information object : 
EVC Source Port
EVC Destination Port
VLAN Circuit ID
External ID
VLAN Speed</description>
        <errorConditionFormula>AND(NOT(ISNEW()), 
$Profile.Name = &apos;SI_EBM_DEV_OPS&apos;, 
OR(ISCHANGED(EVC_Source_Port__c),ISCHANGED(EVC_Destination_Port__c),ISCHANGED(VLAN_Circuit_ID__c),ISCHANGED(External_ID__c),ISCHANGED(VLAN_Speed__c)))</errorConditionFormula>
        <errorMessage>Your profile does not have permission to edit the following fields on this object : EVC Source Port, EVC Destination Port, VLAN Circuit ID, External ID, VLAN Speed</errorMessage>
    </validationRules>
