<script>
$j(function () {
    
    var summaryTitle = $j('.summaryText').text();
    if (summaryTitle .length > 200) {
      var chopText = summaryTitle .trim().substring(0, 200).split(" ").slice(0, -1).join(" ") + "…";
      $j('.summaryText').text(chopText);
    }
     
      var summaryDefTitle = $j('.summaryDefaultText').text();
    if (summaryDefTitle .length > 200) {
      var chopTextDefault = summaryDefTitle .trim().substring(0, 200).split(" ").slice(0, -1).join(" ") + "…";
      $j('.summaryDefaultText').text(chopTextDefault);
    }
    //Mouse Scroll Events
   $j(window).scroll(function() {    
   if($j(window).scrollTop() + $j(window).height() == $j(document).height()) {
       //$j('#mouseScroll').css('display','block');
       mouseScrollFind(); 
       window.onload=toBottom;         
    } 
