    $j(this).find("[id$=providerSectionDefault]").css('display','block');
    $j(this).find("[id$=imageHoverDefault]").css('display','none');
    },
    function(){
        $j(this).find("[id$=providerSectionDefault]").css('display','none');
        $j(this).find("[id$=imageHoverDefault]").css('display','block');
    });
    
    $j("[id$=imageHoverId]").hover(function(){
        $j(this).find("[id$=providerSection]").css('display','block');
        $j(this).find("[id$=mainImage]").css('display','none');
    },
    function(){
        $j(this).find("[id$=providerSection]").css('display','none');
        $j(this).find("[id$=mainImage]").css('display','block');
    });    
    });
     })
 
    function mouseScrollFind(){
        callfind(); 
    }
    function toBottom(){
        window.scrollTo(0, document.body.scrollHeight);
    }
    function toTop(){
        window.scrollTop(0);
    }

<!--scroll top start-->

     $j(document).ready(function(){
        $j(window).scroll(function(){
        if ($j(this).scrollTop() > 100) {
            $j('.scrollToTop').fadeIn();
        } else {
