                                        <apex:selectoption itemValue="AVPN" itemLabel="AVPN - AT&T Virtual Private Network"></apex:selectoption>
                                        <apex:selectoption itemValue="IPFLEX" itemLabel="IPFLEX - IP Flexible Reach (VOIP)"></apex:selectoption>
