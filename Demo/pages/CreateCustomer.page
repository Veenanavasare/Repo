<apex:page standardController="ATOM_End_Customer__c" tabStyle="ATOM_End_Customer__c" extensions="CreateCustomerController">
<apex:form >
<apex:pageMessages ></apex:pageMessages>
<apex:sectionHeader title="End Customer Edit" subtitle="New End Customer"/>
<apex:pageBlock title="End Customer Edit">
<apex:pageBlockSection columns="1">
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="Company Legal Name"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.Company_Legal_Name__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="Street"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.Street__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="City"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.City__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="State/Province"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.State_Province__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="Country"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.Country__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="Zip/Postal Code"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.Zip_Postal_Code__c}"/>
    </apex:pageBlockSectionItem>
    
    <apex:pageBlockSectionItem >
        <apex:outputLabel value="DUNS Number"></apex:outputLabel>
        <apex:inputField value="{!ATOM_End_Customer__c.DUNS_Number__c}"/>
    </apex:pageBlockSectionItem>
    
</apex:pageBlockSection>

<apex:pageBlockButtons >
    <apex:commandButton value="Save" action="{!Save}"/>
    <apex:commandButton value="Cancel" action="{!Cancel}"/>
</apex:pageBlockButtons>

</apex:pageBlock>
</apex:form>
</apex:page>