    function validateTextFieldsPage2(sClassName, sErrorMessage, sBlankMessage, boolIsRequired) {
        var sValue = "",
        oField = $("." + sClassName),
        oParent = oField.parent();
        if (oField[0]) {
        // Check if the field contains blank value as the first character
         sValue = oField.val();
         // Handle dropdown value as well
          if ((boolIsRequired) && ("" === sValue || "None(ARPA)" === sValue)) {
          // Append the error message in case the field is not filled
           if (oParent[0]) {
          // Remove all spaces and () from the string
             $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
              oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>" + sBlankMessage + "</p>");
            }
           } else if ((" " === sValue) || (sValue !== sValue.trimLeft())) {
             // Append the error message in case the field is wrongly filled
              if (oParent[0]) {
              // Remove all spaces and () from the string
               $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
               oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>" + sErrorMessage + "</p>");
              }
            }
          // Check if this is a valid number
            else if (("requiredInputFieldNumber" === sClassName || "requiredInputFieldNumberipv6" === sClassName) && isNaN(sValue)) {
             if (oParent[0]) {
              // Remove all spaces and () from the string
              $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
              oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>Invalid Entry</p>");
             }
           }
          // Number fields - ASN Number
         //else if ("requiredInputFieldNumber" === sClassName && ( parseInt(sValue,10)<64512 || parseInt(sValue,10)>65534))
         else if (("requiredInputFieldNumber" === sClassName || "requiredInputFieldNumberipv6" === sClassName) && (parseInt(sValue, 10) < 64512 || parseInt(sValue, 10) > 64999) && (parseInt(sValue, 10) < 65005 || parseInt(sValue, 10) > 65534)) {
       // Append the error message in case the field is wrongly filled
          if (oParent[0]) {
       // Remove all spaces and () from the string
            $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
            oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>Invalid Entry</p>");
           }
          } else {
         // Remove the error message
            if (oParent[0]) {
                oParent.removeClass("has-error");
         // Remove all spaces and () from the string
                $(".errorText." + sClassName.replace(/\s/g, "_").replace(/[""()]/g, "") + "_error").remove();
               }
           }
         }
        // All Field Check
        checkForErrorsPage2();
      }
