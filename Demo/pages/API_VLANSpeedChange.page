    function GetURLParameter(sParam) {
      var sPageURL = window.location.search.substring(1);
      var sURLVariables = sPageURL.split('&');
       for (var i = 0; i < sURLVariables.length; i++) {
            var sParameterName = sURLVariables[i].split('=');
            if (sParameterName[0] == sParam) {
             return sParameterName[1];
             }
          }
      }    
         
    function progressBar(alreadyused,available, $element,changed,spdchangeindicator) {
        
        
        
        //alert("g"+alreadyused+"h");
        if(alreadyused==null || alreadyused=="" || alreadyused=="undefined")
        {
        return true;
        }
     
     //alert(alreadyused +" = "+available+" = "+ changed+" = "+spdchangeindicator);

    //total speed
     var pSpeed = "{!listDisplayValidationRecords.Port_Speed__c}";
    
     var alreadyusedint = parseInt(alreadyused);
     
     var availableintwithsign = parseInt(available);
     
     var changedintwithsign =  parseInt(changed);
     
     var availableintwithoutsign =  Math.abs(availableintwithsign);
 
     var changedintwithoutsign =   Math.abs(changedintwithsign);
     
     var totaltemp=0;
     
     

     
     
     
     if(spdchangeindicator=="Positive")
         {
         
         
       
         
         var exceeded = (alreadyusedint+changedintwithoutsign)-pSpeed;
         
        
         
         if(exceeded>0)
             {
             
             
               totaltemp=alreadyusedint+changedintwithoutsign//+exceeded;
               //for already used bar - dark grey
               var progressBarWidth = alreadyusedint * 800 / totaltemp;
               $('#addvlanprogbardiv1').animate({width:progressBarWidth}, totaltemp).html();
               $('#Usedvlanspd').html(alreadyusedint+" Mbps");

              
               
              
               
             
               //changed speed - Orange
               var progressBarchangedWidth = changedintwithoutsign * 800 / totaltemp;
               $('#addvlanprogbardiv2').animate({width:progressBarchangedWidth}, totaltemp).html();
               $('#PendVlanSpd').html(changedintwithoutsign+" Mbps");
               $('#penddivtd').show();
               $('#pendtd').show();
               
               
               
               //alert("1 "+changedintwithoutsign);
               
               
              
                 //Exceeded - Red
                $element.find('span').css("background-color", "#b30a3c");
                 var progressBarWidthSpan = (exceeded) * 800 / totaltemp - 1;
                $element.find('span').animate({ width:50}, totaltemp).html();
                
                
                
                
                
                
                
                $('#availtd').hide();
                $('#availdivtd').hide();
                
                $('#exceededtd').show();
                $('#exceeddivtd').show();
                //$('#Availablevlanspd').css("background","#EF0C0C");
                $('#Exceededspd').html(exceeded+" Mbps");
                //$('#avflagkey').html("Exceeded:");
                $('.applynsaveforspdchange').addClass("disabled");
        
                
                return;
             
             }
         
         }

     
     
      
    totaltemp=alreadyusedint+availableintwithoutsign+changedintwithoutsign;
     
     //for already used bar - dark grey
     
       var progressBarWidth = alreadyusedint * 800 / totaltemp;
       $('#addvlanprogbardiv1').animate({width:progressBarWidth}, totaltemp).html();
       $('#Usedvlanspd').html(alreadyusedint+" Mbps");
     
     
    //changed speed - Green
    var progressBarchangedWidth = changedintwithoutsign * 800 / totaltemp;
    
    if(spdchangeindicator=="Positive")
        {
        if(changedintwithoutsign!=0){
        $('#addvlanprogbardiv2').animate({width:progressBarchangedWidth}, totaltemp).html();
        $('#PendVlanSpd').html(changedintwithoutsign+" Mbps");
        $('#penddivtd').show();
        $('#pendtd').show();
        //alert("2 "+changedintwithoutsign);
        
        }
        }
    else
        {
        if(changedintwithoutsign!=0){
        $('#addvlanprogbardiv2').animate({width:progressBarchangedWidth}, totaltemp).html();
        $('#PendVlanSpd').html(changedintwithoutsign+" Mbps");
        $('#penddivtd').show();
        $('#pendtd').show();
        //alert("3 "+changedintwithoutsign);
        
        }
        }
    
    
    
    
    //Available - light grey
    var progressBarWidthSpan = (availableintwithoutsign) * 800 / totaltemp - 1;
    $element.find('span').animate({ width:progressBarWidthSpan}, totaltemp).html();
    $('#Availablevlanspd').html(availableintwithoutsign+" Mbps");
    $('#availtd').show();
    $('#availdivtd').show();
    $('#exceededtd').hide();
    $('#exceeddivtd').hide();
    
    
    
    
    
    

