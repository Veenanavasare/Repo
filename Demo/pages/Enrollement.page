         <script>    
            function CustomEmailCheck (emailValue) {
                var eValue = emailValue; 
                if (eValue == ""){
                    $("#EmailError").remove();
                    $(".email-addr-input").after("<p id='EmailError' class='errorText'>Cannot be blank</p>" );
                }
                else if(eValue != ""){
                    if(!isValidEmailAddress(eValue) && eValue != "") {
                        $("#EmailError").remove();
                        $(".email-addr-input").after("<p id='EmailError' class='errorText'>Please enter a valid email</p>" );
                    } else {
                        $("#EmailError").remove();
                    }
                }
             }
                        // Function to initialize each of the validations present on the form fields
           
                 // Function to handle numerical values
                // If any non - numerical characters are entered, we show an invalid entry message
                // If a required field is blank, we show a message indicating that the field is blank
                // If a phone number is greater or less than 10 digits in length, we show a message indicating that the number is longer than expected
             function validateNumericalFields(sValue, sErrorMessage, sBlankMessage, sPhoneDigitErrorMessage,boolIsRequired, sParentID, sErrorFieldID, sFirstSpaceErrorMessage){
                var oParent = $("#"+sParentID+""), oError = $("#"+sErrorFieldID+"");

                if(oParent[0]) {
                   if((boolIsRequired) && ("" === sValue))
                    {
                        oParent.addClass("has-error");
                        if(oError){
                            oError.remove();
                        }

                        oParent.append("<p id='"+sErrorFieldID+"' class='errorText'>"+sBlankMessage+"</p>" );
                    }
                    else if((" " === sValue) || (sValue !== $.trim(sValue))){
                        oParent.addClass("has-error");
                        if(oError){
                            oError.remove();
                        }

                        oParent.append("<p id='"+sErrorFieldID+"' class='errorText'>"+sFirstSpaceErrorMessage+"</p>" );
                    }
                    else if(isNaN(sValue)){
                        oParent.addClass("has-error");
                        if(oError){
                            oError.remove();
                        }

                        oParent.append("<p id='"+sErrorFieldID+"' class='errorText'>"+sErrorMessage+"</p>" );
                    }
                     
                    
                       else if(("phoneNumberDiv" === sParentID || "phoneNumberDivId" === sParentID) && ( sValue.length>10 || sValue.length < 10)){
                        oParent.addClass("has-error");
                        if(oError){
                            oError.remove();
                        }
                        oParent.append("<p id='"+sErrorFieldID+"' class='errorText'>"+sPhoneDigitErrorMessage+"</p>" );

                    }
                    else{
                         oParent.removeClass("has-error");
                         if(oError){
                            oError.remove();
                        }
                    }
                }
             
                // All Field Check
                validateRequiredFields();
            }
                        // Function to check whether the field is empty or whether the field contains preceeding white space
                        // If the field contains white space or preceeding white space, we show an error message
                        // If a required field is blank, we show a message indicating that the field is blank
            function validateEmptyFields (sStyleClass,sErrorMessage, sBlankMessage, boolIsRequired) {
                var sValue = $("."+sStyleClass+"").val(), oField = $("."+sStyleClass+""), oParent = oField.parent();
            
                if(oField[0])
                {
                    // Check if the field contains blank value as the first character
                    sValue = oField.val();
                    if((boolIsRequired) && ("" === sValue))
                    {
                        // Append the error message in case the field is not filled
                        if(oParent[0]){
                            // Remove all spaces and () from the string
                            $(".errorText."+sStyleClass.replace(/\s/g,"_").replace(/["'()]/g,"")+"_error").remove();
                            oParent.addClass("has-error").append("<p class='errorText "+sStyleClass.replace(/\s/g,"_").replace(/["'()]/g,"")+"_error'>"+ sBlankMessage +"</p>");
                        }
                    }
                    else if((" " === sValue) || (sValue !== $.trim(sValue)))
                    {
                        // Append the error message in case the field is wrongly filled
                        if(oParent[0]){
                            // Remove all spaces and () from the string
                            $(".errorText."+sStyleClass.replace(/\s/g,"_").replace(/["'()]/g,"")+"_error").remove();
                            oParent.addClass("has-error").append("<p class='errorText "+sStyleClass.replace(/\s/g,"_").replace(/["'()]/g,"")+"_error'>"+ sErrorMessage +"</p>");
                        }
                    }
                    else
                    {   
                        // Remove the error message
                        if(oParent[0]){
                            oParent.removeClass("has-error");
                            // Remove all spaces and () from the string
                            $(".errorText."+sStyleClass.replace(/\s/g,"_").replace(/["'()]/g,"")+"_error").remove();
                        }
                    }
                }
                // All Field Check
                validateRequiredFields();
            }
            
             //If a ZipCode is greater or less than 5 digits in length, we show a message indicating that the number is longer than expected
             
             
             function validateZipCode(thisEle) {
               if (isNaN(thisEle.value)) {
                $(thisEle).addClass("has-error");
                $('#ZipCodeErrorText').remove();
                $(thisEle).after("<p id='ZipCodeErrorText' class='errorText'>Invalid ZipCode</p>");

            } else 
             if (0 !== thisEle.value.length && (!isNaN(thisEle.value)) && (thisEle.value.length > 5 || thisEle.value.length < 5)) {
                $(thisEle).addClass("has-error");
                $('#ZipCodeErrorText').remove();
                $(thisEle).after("<p id='ZipCodeErrorText' class='errorText'>Must be 5 digits</p>");
            } 
            else {
                $(thisEle).removeClass("has-error");
                $('#ZipCodeErrorText').remove();
            }

         }                    
      function isValidEmailAddress(emailAddress) {
    var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
    return pattern.test(emailAddress);
    }        
                function validateRequiredFields () {
                // Iterate through all fields and check if there are any invalid entries. If yes, then return false    
                var arrRequiredFields = $(".required"), boolIsNumberValid = true,boolIsValid = true;
                var state = $(".apply_state").val();     
                $(".apply_state").on("focusout", function () {
                    if(state == "Select") {
                        $(".state-error").remove();
                        $(".apply_state").after("<div class='has-error errorText state-error'>Selection required</div>");
                    } else {
                        $(".state-error").remove();
                    }    
                });
                arrRequiredFields.each(function () 
                {   
                    oRequiredField = $(this);
                    if(oRequiredField[0]) 
                    {
                        if (0 === oRequiredField.val().length)
                        {
                            boolIsValid = false;
                            return false;
                        }
                    }
                }); 
           
                // If there is any error message present, do not enable the button
                if(0 < $(".has-error").length)
                {
                    boolIsNumberValid = false;
                }
                if(boolIsValid && boolIsNumberValid){
                    if($("#check").is(":checked")){
                      $("#myButton").removeAttr("disabled").addClass("enabled"); 
                    }
                }
                else
                {   
                    $("#myButton").attr("disabled","disabled").removeClass("enabled");
                }  

     }  
     </script>  
