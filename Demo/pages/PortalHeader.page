                if (type === 'hasSub') {
                    thisHasSub(text);
                } else {
                    window.scrollTo(0, 0);
                    j$('.sub_navigation').css({
                        'height': '0px'
                    });
                    j$('.' + text + '-content').css({
                        'display': 'block',
                        'height': 'auto'
                    });
                    j$('.' + text + '-content').siblings().css({
                        'display': 'none'
                    });
                    j$('#nav_desktop a').removeClass('active_top');
                    if (text === 'dashboard' ||
                        text === 'developers') {
                        j$('.top_link_' + text).removeClass('unactive_top');
                        j$('.top_link_' + text).addClass('active_top');
                    }
                }
            });

            function thisHasSub(text) {
                if (j$('.' + text + '-content').css('display') === 'block') {
                    j$('.' + text + '-content').css({
                        'display': 'none'
                    });
                    j$('.top_link_' + text).removeClass('active_top');
                    j$('.top_link_' + text).addClass('unactive_top');
                    j$('.sub_navigation').animate({
                        height: '0px'
                    });
                } else {
                    window.scrollTo(0, 0);
                    j$('.sub_navigation').css({
                        'height': '0px'
                    });
                    j$('.' + text + '-content').css({
                        'display': 'block',
                        'height': 'auto'
                    });
                    j$('.' + text + '-content').siblings().css({
                        'display': 'none'
                    });
                    j$('#nav_desktop a').removeClass('active_top');
                    j$('.top_link_' + text).removeClass('unactive_top');
                    j$('.top_link_' + text).addClass('active_top');
                    contentHeight = j$('.' + text + '-content').innerHeight();
                    j$('.sub_navigation').animate({
                        height: contentHeight
                    }, function () {
                        j$('.sub_navigation').css({
                            'height': 'auto'
                        });
                        j$('.' + text + '-content').css({
                            'height': 'auto'
                        });
                    });
                }
            }
