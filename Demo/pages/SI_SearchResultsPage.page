                                            <div class="listViewOpacityOverlay">
                                                <a style="cursor: pointer; margin-right:15px;" href="" onclick="return opener('{!JSINHTMLENCODE(status.Title)}','{!status.HLS_Video_URL__c}','{!status.HDS_Video_URL__c}','{!JSINHTMLENCODE(status.Summary)}')" value="#">
                                                    <apex:image styleClass="listViewPlayIcon" value="{!URLFOR($Resource.newsroombanners, '32066-Slices-and-Guide/Slices/icon-play.png')}" width="35px" height="35px"/>
                                                </a>
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                   </div> 
