    <validationRules>
        <fullName>Banner_Active_Validation</fullName>
        <active>true</active>
        <description>Banner Active Validation status is Active than Image should uploead</description>
        <errorConditionFormula>AND(
ISPICKVAL(Banner_Status__c, &quot;Active&quot;),
OR(
Banner_Link__c=NULL
)
)</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Please upload an Banner image to mark the Banner Status as Active</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Cannot_be_Less_Than_Start_Date</fullName>
        <active>true</active>
        <description>End date cannot be less than start date</description>
        <errorConditionFormula>OR(AND(
ISNEW(),End_Date__c &lt; Start_Date__c ,
RecordType.Name=&quot;Event&quot;
),
AND(ISCHANGED(End_Date__c),
 End_Date__c &lt; Start_Date__c,
RecordType.Name=&quot;Event&quot;))</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date cannot be prior to Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Time_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT(ISBLANK( End_Time__c)), NOT(
REGEX(  End_Time__c  , &apos;^(([0][0-9]|[1][0-2])|[0-9]):([0-5][0-9])( *)((AM|PM)|(A|P))$&apos;)
))</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>Invalid time format. Please use HH:MM AM/PM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_City_and_State_for_Events</fullName>
        <active>true</active>
        <description>User will be required to enter City and State when publishing Event</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(Status__c, &quot;Published&quot;),
ISBLANK(City__c), 
ISBLANK(State__c ),
RecordType.DeveloperName = &quot;Event&quot;)</errorConditionFormula>
        <errorMessage>In order to publish this Event, City and State must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Descrip_for_Events_and_Spnships</fullName>
        <active>true</active>
        <description>User will be required to enter description when publishing Event or Sponsorship record</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(Status__c, &quot;Published&quot;),
ISBLANK( Description__c ), 
OR( RecordType.DeveloperName = &quot;Event&quot;, RecordType.DeveloperName = &quot;Sponsorship&quot;))</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>In order to publish this record, a description must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Image_for_Events</fullName>
        <active>true</active>
        <description>User will be required to upload image when publishing Event</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(Status__c, &quot;Published&quot;),
ISBLANK( Event_Image_Link__c ),
RecordType.DeveloperName = &quot;Event&quot;)</errorConditionFormula>
        <errorMessage>In order to publish this Event, an image must be uploaded</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Start_and_End_Dates_for_Events</fullName>
        <active>true</active>
        <description>User will be required to enter Start and End Dates when publishing Event</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(Status__c, &quot;Published&quot;),
ISBLANK(Start_Date__c ), 
ISBLANK(End_Date__c ),
RecordType.DeveloperName = &quot;Event&quot;)</errorConditionFormula>
        <errorMessage>In order to publish this Event, Start and End Dates must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Website_for_Events_and_Spnships</fullName>
        <active>true</active>
        <description>User will be required to enter website when publishing Event or Sponsorship record</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(Status__c, &quot;Published&quot;),
ISBLANK( Website__c), 
OR( RecordType.DeveloperName = &quot;Event&quot;, RecordType.DeveloperName = &quot;Sponsorship&quot;))</errorConditionFormula>
        <errorDisplayField>Website__c</errorDisplayField>
        <errorMessage>In order to publish this record, a website must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Time_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (NOT(ISBLANK(Start_Time__c)), NOT(
REGEX( Start_Time__c , &apos;^(([0][0-9]|[1][0-2])|[0-9]):([0-5][0-9])( *)((AM|PM)|(A|P))$&apos;)
))</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Invalid time format. Please use HH:MM AM/PM</errorMessage>
    </validationRules>
