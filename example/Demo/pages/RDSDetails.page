    <script>
        var conResponse;
        var count=0;   
        var zipFile = new JSZip();
        var jsAtt;
        var zipFileCount = 1;
        var arr = new Array();
        var totalFileSize=0;
        var lastAttachment=0;
        var attachmentCounter = 0;
        var dataTableComp;
        var downloadDataTableComp;
        var chkValArr=[];
        var singleFileDownloadArr = [];
        var fileSiteName='{!siteName}';
        var index=0;
        var hideProcessing=false;
        
        function downloadRDSDetails(){
            if(typeof $('#walkme-player') != 'undefined'){
                $('#walkme-player').hide();
            }
            var isVLAN = false;
            var nonCollapsableBlock = $('.panel-heading.active').attr('id');
            if($('#vlanMultiBlock').length>0){
                isVLAN = true;
                $('.collapse').collapse('show');
            }   
            setTimeout(function(){
            var canvasHeight = $(document).height();
            var canvasWidth = $(document).width(); 
            var pdf = new jsPDF('p', 'pt', [canvasHeight,canvasWidth]);
            pdf.addHTML(document.body, function() {
                pdf.save(fileSiteName+'_RDSDetails.pdf');
                if(isVLAN){
                    setTimeout(function(){
                        $('.collapse.in').collapse('hide');
                        if(typeof nonCollapsableBlock != 'undefined'){
                            $('[aria-labelledby='+nonCollapsableBlock+']').collapse('show');
                        }
                    },200); 
                }
                if(typeof $('#walkme-player') != 'undefined'){
                $('#walkme-player').show();
            }
            });
            },300);
            return false;
        }

        function downloadZip(){
            zipFileCount =1;
            totalFileSize=0;
            attachmentCounter = 0;
            var chkValArr = {!attachmentIdList};
            var fileName = '';
            $("#loading").parent().show();
            if(chkValArr.length>1){
                chkValArr.forEach(function(attId) {
                     Visualforce.remoting.Manager.invokeAction(
                     '{!$RemoteAction.RDSDetailsController.getDocumentCnt}',
                     attId,
                     function(result,event) 
                     {
                         if(event.status)
                         {
                             jsAtt = result;
                             var attachmentData;
                             hideProcessing = true;
                             $.each(jsAtt, function(key,value) 
                             {
                                                
                                totalFileSize+=value.attFileSize;
                                
                                if(attachmentCounter==0){
                                    zipFile = new JSZip();
                                    fileName = fileSiteName+'_Archive Documents'+zipFileCount+'.zip';
                                    
                                }
                                attachmentCounter++;
                                if (totalFileSize <26203910)
                                {
                                    compressFile(zipFile,value.attName,value.attEncodedBody);
                                    if(attachmentCounter ==  chkValArr.length){
                                        setTimeout(function(){
                                            attachmentData = zipFile.generate();
                                            download('data:application/zip;base64,'+attachmentData,fileName,'application/zip');
                                        },300); 
                                        setTimeout(function(){$("#loading").parent().hide();},1000);
                                        hideProcessing = false;
                                    }
                                }  
                                else{
                                    zipFileCount++;
                                    totalFileSize=value.attFileSize;
                                    attachmentData = zipFile.generate();
                                    download('data:application/zip;base64,'+attachmentData,fileName,'application/zip');  
                                    zipFile = new JSZip();
                                    fileName = fileSiteName+'_Archive Documents'+zipFileCount+'.zip';
                                        compressFile(zipFile,value.attName,value.attEncodedBody);
                                        if(attachmentCounter ==  chkValArr.length){ 
                                            setTimeout(function(){
                                            attachmentData = zipFile.generate();
                                            download('data:application/zip;base64,'+attachmentData,fileName,'application/zip'); 
                                            setTimeout(function(){$("#loading").parent().hide();},1000);
                                            hideProcessing = false;
                                            },300);  
                                        }
                                    
                                }
                                 
                                
                             });
                             
                         }
                         
                    },{buffer: false,escape: true, timeout: 90000});
                 });
             }
             else if(chkValArr.length ==1){
                if(typeof ($('[id='+chkValArr[0]+']'))[0] != 'undefined'){
                    ($('[id='+chkValArr[0]+']'))[0].click();
                }
                ($('[id='+chkValArr[0]+']')).trigger("click");
                setTimeout(function(){$("#loading").parent().hide()},1000);                                  
            }
             
             return false;
         }

        function compressFile(zipFile, name, data){
            zipFile.file(name, data, {base64:true});
        }

        var jumpToPosition = 0;
        $(document).ready(function(){
            jumpToPosition = $("#jumpTo").offset().top;
            var ipFlexInformationFlag = '{!ipFlexInformation}';
            console.log(typeof ipFlexInformationFlag+'  :: '+ipFlexInformationFlag);
            if(ipFlexInformationFlag == "false")
            {
                $("#downloadDocumentId").attr("disabled","true"); 
            }
            $('.scrollToTop').click(function(){
                $('html, body').animate({scrollTop : 0},500);
                return false;
            });
            $(window).scroll(function(){
                if ($(this).scrollTop() > 200) {
                    $('.scrollToTop').fadeIn();
                } else {
                    $('.scrollToTop').fadeOut();
                }
            });
            window.addEventListener("hashchange", function () {
                window.scrollTo(window.scrollX, window.scrollY-100);
            });
            $('.uploadedFileLink a').each(function(){
                var str = $(this).text();
                if (str.length > 50) {
                    str = str.substr(0, 25) + '...' + str.substr(str.length-25, str.length);
                    $(this).text(str);
                }
            }); 
            
        });
        $( window ).bind( "load", function() {  

            /* Toggle VLAN */
            $('.panel-collapse').on('show.bs.collapse', function () {
                $(this).siblings('.panel-heading').addClass('active');
            });

            $('.panel-collapse').on('hide.bs.collapse', function () {
                $(this).siblings('.panel-heading').removeClass('active');
            });

        });

    </script>

