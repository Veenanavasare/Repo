                });
            }
        } 
        var maskIpVal,LANIpVal,numLANBlockVal,messID,errorMessDiv,maskCount,lanIpAdd;
        
        function validateMaskIpAddress(maskIp){
            maskIpVal = maskIp.value;
            maskCount = maskIp.id;
        }
        
        function numberOfLANBlock(numLANBlock){
            numLANBlockVal = numLANBlock.value;
        }
        function validateLANIpAddress(ipAddObj) {

           LANIpVal = ipAddObj.value;
           lanIpAdd = ipAddObj.id;
           
           lanIndex = LANIpVal.split(".");
           var z = lanIndex[lanIndex.length-1];
           //var z = lanIndex.substr(12,15);
           var res ;
           if(maskIpVal != null && maskIpVal.length>15) res  = maskIpVal.substr(12,15);
           var maskCountVal;
           if(maskCount!= null && maskCount.length>66) maskCountVal = maskCount.substr(66);

           if(res == '254'){
              z= z%2;              
           }
        
           if(res == '252'){
              
              z= z%4;
           }
        
           if(res == '248'){
              
              z= z%8;
           }
        
           if(res == '240'){
              
              z= z%16;
           }
        
           if(res == '224'){
            
              z= z%32;
           }
        
           if(res == '192'){
              
              z= z%64;
           } 
        
           if(res == '128'){
              
              z= z%128;
              
           }
           if( z!= 0){
               
               errorMessDiv = lanIpAdd.replace('ipAddressId' , 'errorMessID');
               document.getElementById(errorMessDiv).style.display="block"; 
               $('input[id$=submitButtonId]').css("background-color", "#428bca");
               $('input[id$=submitButtonId]').css("cursor","default");
               $('input[id$=submitButtonId]').prop("disabled",true);
           }
           
           else{
               
               errorMessDiv = lanIpAdd.replace('ipAddressId' , 'errorMessID');
               document.getElementById(errorMessDiv).style.display="none";
               //document.getElementById("pagecontainer:j_id31:test:j_id173:j_id174:j_id178:j_id205:submitButtonId").style.backgroundColor="#056fab";
