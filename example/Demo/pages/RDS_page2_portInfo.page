        function validateMD5PasswordField(pValue,errMsg){
        var flag = false;
            //alert("MD5 password:"+pValue);
            
            if ("" === pValue) {
                errMsg.innerHTML = 'Password cannot be blank';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
                flag = true;
            }else if(pValue.length<8){
                 errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                 jQuery('[id$=pwdv4conf]').prop('disabled', true);
                 flag = true;
            }else if(pValue.search(/[A-Z]/) < 0){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
                flag = true;
            }else if (pValue.search(/[0-9]/) < 0) {
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
                flag = true;
            }else{
                errMsg.innerHTML = '';
                jQuery('[id$=pwdv4conf]').prop('disabled', false);
            }
            if(flag)
            {
            //console.log('flag>>>>'+flag);
               //jQuery('input[id$=lastsubmitButtonId]').removeClass("btn btn-primary m-r-10 btnContinuePage2 btnContinue");
               jQuery('input[id$=saveBtnId1]').removeClass("btn btn-primary m-r-10 rdsSubmit");
               jQuery('input[id$=saveBtnId1]').addClass("btn btn-primary m-r-10 rdsSubmit disabled");
            }
            
        }

        function validateMD5PasswordFieldV6(pValue,errMsg){
            //alert("MD5 password:"+pValue);
            var flag = false;
            if("" === pValue) {
                errMsg.innerHTML = 'Password cannot be blank';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
                flag = true;
            }else if(pValue.length<8){
                 errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                 jQuery('[id$=pwdv6conf]').prop('disabled', true);
                 flag = true;
            }else if(pValue.search(/[A-Z]/) < 0){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
                flag = true;
            }else if (pValue.search(/[0-9]/) < 0) {
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
                flag = true;
            }else{
                errMsg.innerHTML = '';
                jQuery('[id$=pwdv6conf]').prop('disabled', false);
            }
            if(flag)
            {
               jQuery('input[id$=saveBtnId1]').removeClass("btn btn-primary m-r-10 rdsSubmit");
               jQuery('input[id$=saveBtnId1]').addClass("btn btn-primary m-r-10 rdsSubmit disabled");
            }
        }
        
