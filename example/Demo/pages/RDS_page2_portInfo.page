               $('input[id$=submitButtonId]').css("background-color", "#056fab");
               $('input[id$=submitButtonId]').css("cursor","pointer");
              //document.getElementById("pagecontainer:j_id31:test:j_id173:j_id174:j_id178:j_id205:submitButtonId").disabled=false;
               $('input[id$=submitButtonId]').prop("disabled",false);
           }
        }
