    function ReFormatSelectListOptions(token){
        var selectEle = $("#MacdSelectListId>ul>li");
        for(var i = 0; i<selectEle.length;i++){
            if(selectEle.eq(i).text() != token){
                selectEle.eq(i).children('a').addClass('click_disable');
                selectEle.eq(i).children('a').removeClass('click_enable');
            }
        }
    }
    
