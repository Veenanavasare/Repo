    function progressBar(percent, $element) {
        var pSpeed = "{!portSpeed}";
        var progressBarWidth = percent * $element.width() / pSpeed;
        $element.find('div').animate({width:progressBarWidth}, pSpeed).html();
        
        var progressBarWidthSpan = (pSpeed-percent) * $element.width() / pSpeed;
        $element.find('span').animate({ width: progressBarWidthSpan }, pSpeed).html();

        if((pSpeed-percent)==0){
            
            document.getElementById("avlblId").style.display="none";
        }else{
            
            document.getElementById("avlblId").style.display="block";
        }
