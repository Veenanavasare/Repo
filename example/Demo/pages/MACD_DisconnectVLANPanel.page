      function hideButton(){
             document.getElementById("myButton").disabled=true;
      }
      
      function SetButtonStatus(sender)
      {
          
        if ( sender == 0 )
        {
            document.getElementById("myButton").disabled=true;
            $("#myButton").css("background","#ccc");
        }
        else{
                document.getElementById("myButton").disabled=false;
                $("#myButton").css("background","#056fab");
            }
      } 
         
        
    function progressBar_INV(percent, pending, $element) {
        var pSpeed = "{!valPortSpeed}";
        
        var progressBarWidth = percent * $element.width() / pSpeed;
        $('#used_INV').animate({width:progressBarWidth}, pSpeed).html();
        if(pending != 0 ){
            var pedningBarWidth = (parseInt(pending)) * $element.width() / pSpeed;
            $('#pending_INV').animate({width:pedningBarWidth}, pSpeed).html();
        }else{
                
            var pedningBarWidth = (parseInt(pending)) * $element.width() / pSpeed;
            $('#pending_INV').animate({width:pedningBarWidth}, pSpeed).html("");
        }
        var progressBarWidthSpan = (pSpeed-percent-pending) * $element.width() / pSpeed;
        $element.find('span').animate({ width: progressBarWidthSpan }, pSpeed).html();
        
     
        if(( parseInt(pSpeed) - parseInt(percent))==0){
         
            document.getElementById("avlblId_INV").style.display="none";
        }else{
          
            document.getElementById("avlblId_INV").style.display="block";
        }
    }
      
