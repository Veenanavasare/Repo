         //alert(j+"-"+valConfigArray[j]);
         if(valConfigArray[j] != vid){
            tempValConfigArray[m++] = valConfigArray[j];
         }
       }
       valConfigArray = tempValConfigArray;
       //alert(valConfigArray.length);
    }
  
