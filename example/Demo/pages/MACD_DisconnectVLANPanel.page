                        text: "The minimum number of VLANs for your site is 2. You will have to click Undo Disconnect on one of the pending VLANs to proceed."
                    });
       }else{
           //disconVlanmethod(vid);
           document.getElementById("undoDiscon"+suffix).style.display="block";
           document.getElementById("discon"+suffix).style.display="none";
           
           var divTxt = $('#mbps'+suffix).text();
           $('#mbps'+suffix).html(divTxt+" - Disconnect Pending...");
           //  $('#mbps'+suffix).html(mbpsVal+" Mbps"+" - Disconnect Pending...");
           document.getElementById('mbps'+suffix).style.color = "#FFA500"; 
           document.getElementById("disConnectPage2Div").style.display="block";
           noOfDisConVLAN++;
           totalMbpdDisConVLAN = totalMbpdDisConVLAN + mbpsVal;
            $('#pendingDisVLAN').html(totalMbpdDisConVLAN+" Mbps");
           var mssg = "<font style=\"font-family:omnes_att_iimedium;font-size:1.15em;font-weight:600;\">You are disconnecting "+noOfDisConVLAN+" VLAN that uses "+totalMbpdDisConVLAN+" Mbps of available port speed.</font>";
           $('#msg').html(mssg);
           var tUsedSpeed = "{!valTotalUsedSpeed}";
           tUsedSpeed = tUsedSpeed - totalMbpdDisConVLAN;
           $('#usedspeedVLAN').html(tUsedSpeed+" Mbps");           
           progressBar_INV(tUsedSpeed, totalMbpdDisConVLAN, $('#progressBar_INV'));
           SetButtonStatus(noOfDisConVLAN);
           valConfigArray.push(vid);

        //   alert("DisConnect no of items >>> "+ valConfigArray.length);
         //  myselectMethod();
         
      }    
         
    }
      function clickUndoDisCon(prefix, suffix, mbpsVal,vid){
       //alert('clickUndoDisCon');
       document.getElementById("undoDiscon"+suffix).style.display="none";
       document.getElementById("discon"+suffix).style.display="block";
       
       var divTxt = $('#mbps'+suffix).text();
       var mbpsTxt = divTxt.split("-")[0];
       $('#mbps'+suffix).html(mbpsTxt);
       document.getElementById('mbps'+suffix).style.color = "#2A6496"; 
       noOfDisConVLAN--;
       if(noOfDisConVLAN == 0){
        document.getElementById("disConnectPage2Div").style.display="none";  
       }
       
