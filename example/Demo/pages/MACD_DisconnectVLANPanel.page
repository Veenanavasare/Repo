       function clickDisConLink(prefix, suffix, mbpsVal,vid){
       
        //alert(">>>>>>>>prefix>>>>>>>>>>>>"+eval(k));
         
         $('#disconnectLinkDiv').css('background', '#D3D3D3');

       var inputs = document.getElementsByTagName("table");
       var count = 0;
