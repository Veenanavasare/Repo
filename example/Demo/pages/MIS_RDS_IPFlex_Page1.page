<apex:page controller="RDSMISController" standardStylesheets="false" sidebar="false" showHeader="false" tabStyle="Apttus_Proposal__Proposal__c" docType="html-5.0" applyHtmlTag="false" applyBodyTag="false">
    <style>
        #POTSRadio td:first-child label{
            margin-right: 50px;
        }
        #POTSRadio td:first-child{
            width: 18%;
        }        
        .SitesPhnNo label{
            margin-top: 0 !important;
        }
        .DomainURL .errorText{
            margin-top:10px;
        }
        .noteData{
            margin-top: 10px;
            float: left;
        }
        .styled-select .m-t-10{
            position: absolute;
        }
        .IPType fieldset label, .ASNProvider fieldset label{
            margin-right: 21px;
        }        
        .IPType fieldset input[type="radio"]{
            margin: 10px 10px 0 0 !important;
        }
        .IPType fieldset td:first-child{
            width:30%;
        }
        .additionalField {
            margin-top: -15px!important;
        }
        .CRDDField {
            width: 264px;
        }
        .CRDDNote {
            margin-left: 16px;
        }
        .dualStackNote {
             font-size: 12px!important;
             margin: 0!important;
        }
        .dualStackInfoTxt {
             color: #ea7400 !important;
             font-family: omnes_att_iimedium;
             font-size: 12px!important;
        }
        #panelFooter {
            width: 1127px;
            margin-left: -291px!important;
            margin-bottom: 40px !important;
            margin-top: 2px;
        }
        .interfacetypeNote {
            width: 274px;
        }
        .tooltip-inner{
            max-width: 330px!important;
        }
        .IPV6LanLenghtIcon {
            margin-left: -10px!important;
        }
        .interfaceType {
            padding: 7px 25px 7px 6px !important;
            -moz-appearance: none;
            -webkit-appearance: none;
            font-size:14px;
        }
        .intractiveIcon {
            margin-left: 7px;
        }
        .additionalInfoSection{
            margin-bottom: 20px;
        }
        /* Site Info placeholder font*/
        input.siteInfoPlaceholder::-webkit-input-placeholder {
           font-size: 13px;
        }
        
        input.siteInfoPlaceholder::-moz-placeholder { /* Firefox 19+ */
          font-size: 13px;
        }
        
        input.siteInfoPlaceholder:-ms-input-placeholder { /* IE 10+ */
          font-size: 13px;
        }
        
        input.siteInfoPlaceholder:-moz-placeholder { /* Firefox 18- */
          font-size: 13px;
        }
        .siteInfoLabel{
            font-family: omnes_att_iimedium;
            font-size:15px;
        }
        .siteInfoInOrange{
            font-family: omnesatt_iiregular;
            font-weight: 700;
            font-Size: 16px;
            font-color:#ea7400;
        }
        .fontStyle{
            font-weight: 400;
            font-size: 24px;
            line-height: 26px;
        }
        
    </style>
    <apex:form styleClass="MISRDSPage1">    
        <!-- Begin Site Information -->
        <apex:outputPanel id="addressInfoSection">
            <p class="orangeText m-b-25 siteInfoInOrange">{!$Label.RDS_BUILDING_STATICMSG}</p>            
            <div class="row">
                <div class="form-group col-md-4 m-b-25">
                    <div class="m-b-10 siteInfoLabel">Building</div>
                    <apex:inputText id="buildingID" styleClass="form-control required siteInfoPlaceholder" Label="Building" maxlength="10" html-placeholder="Ex: Bldg #, Bldg name, or Main" value="{!inpBuilding}" onkeyup="fieldValidationForFirstPage(event,this,'siteInfoBldg');" onblur="fieldValidationForFirstPage(event,this,'siteInfoBldg');"/>
                </div>
                <div class="form-group col-md-4 m-b-25">
                    <div class="m-b-10 siteInfoLabel">Floor</div>
                    <apex:inputText id="floorID" styleClass="form-control required siteInfoPlaceholder" label="Floor" maxlength="9" html-placeholder="Ex: Floor #" value="{!inpFloor}" onkeyup="fieldValidationForFirstPage(event,this,'siteInfoFloor');" onblur="fieldValidationForFirstPage(event,this,'siteInfoFloor');"/>
                </div>
                <div class="form-group col-md-4 m-b-25">
                    <div class="m-b-10 siteInfoLabel">Room</div>
                    <apex:inputText id="roomID" styleClass="form-control required siteInfoPlaceholder" label="Room" maxlength="9" html-placeholder="Ex: Room #, Telco, IT Closet, Basement" value="{!inpRoom}" onkeyup="fieldValidationForFirstPage(event,this,'siteInfoRoom');" onblur="fieldValidationForFirstPage(event,this,'siteInfoRoom');"/>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-4 m-b-10" id="siteLandLinePhnNum">
                    <div class="m-b-10 siteInfoLabel">Site Phone Number</div>
                    <apex:inputText styleClass="form-control required" value="{!sitephonenumber}" html-placeholder="0000000000" onkeyup="fieldValidationForFirstPage(event,this,'siteLandLinePhnNum');" onblur="fieldValidationForFirstPage(event,this,'siteLandLinePhnNum');"/>
                </div>        
            </div>
        </apex:outputPanel> 
        
        <hr class="section-dividerHr" />
        
        <!-- Begin Site Contact -->
        <apex:outputPanel id="siteContactSection">
            <div class="sectionHeader-primary m-b-25 fontStyle">Site Contact</div>
            <!-- Primary Contact -->
            <label>Primary Contact </label><span class="tooltipTrigger glyphicon glyphicon-question-sign intractiveIcon" data-toggle="tooltip" data-placement="right" title="Tell us about the primary local contact for this site."></span>                                    
            <div class="row">
                <div class="form-group col-md-4 m-b-25" id="firstName">
                    <div class="m-b-10 siteInfoLabel">First Name</div>
                    <apex:inputText styleClass="form-control required" value="{!sitecontactfirstname}" id="FirstName" label="First Name" onkeyup="fieldValidationForFirstPage(event,this,'primaryFirstName');" onblur="fieldValidationForFirstPage(event,this,'primaryFirstName');"/>
                </div>
                <div class="form-group col-md-4 m-b-25" id="lastName">
                    <div class="m-b-10 siteInfoLabel">Last Name</div>
                    <apex:inputText styleClass="form-control required" value="{!sitecontactlastname}" label="Last Name" onkeyup="fieldValidationForFirstPage(event,this,'primaryLastName');" onblur="fieldValidationForFirstPage(event,this,'primaryLastName');"/>
                </div>
                <div class="form-group col-md-4 m-b-25" id="email">
                    <div class="m-b-10 siteInfoLabel">Email Address</div>
                    <apex:inputtext styleClass="form-control required" value="{!sitecontactemailaddress}" label="Email Address" title="Please enter an valid email address" onkeyup="fieldValidationForFirstPage(event,this,'primaryEmail');" onblur="fieldValidationForFirstPage(event,this,'primaryEmail');"/>
                </div>
            </div>
            <div class="row">
                <div class="form-group col-md-4 m-b-25" id="phoneNumber">
                    <div class="m-b-10 siteInfoLabel">Business Phone Number</div>
                    <apex:input styleClass="form-control required" value="{!sitecontactphonenumber}" label="Phone Number" type="tel" onkeyup="fieldValidationForFirstPage(event,this,'primaryBussinessPhoneNumber');" onblur="fieldValidationForFirstPage(event,this,'primaryBussinessPhoneNumber');"/>
                </div>
                <div class="form-group col-md-4 m-b-25" id="mobileNumber">
                    <div class="m-b-10 siteInfoLabel">Mobile Phone Number</div>
                    <apex:inputText styleClass="form-control required" value="{!sitecontactmobile}" label="Mobile Phone Number" onkeyup="fieldValidationForFirstPage(event,this,'primaryMobilePhoneNumber');" onblur="fieldValidationForFirstPage(event,this,'primaryMobilePhoneNumber');"/>
                </div>
            </div>                    
            <!-- Alternate Contact -->
            <label>Alternate Contact </label><span class="tooltipTrigger glyphicon glyphicon-question-sign intractiveIcon" rel="tooltip" data-toggle="tooltip" data-placement="right" title="Enter a backup local contact for this site."></span>
            <div class="row">
                <div class="form-group col-md-4 m-b-25" id="firstName">
                    <div class="m-b-10 siteInfoLabel">First Name</div>
                    <apex:inputText styleClass="form-control required" value="{!sitealtcontactfirstname}" label="First Name" onkeyup="fieldValidationForFirstPage(event,this,'secondaryFirstName');" onblur="fieldValidationForFirstPage(event,this,'secondaryFirstName');"/>
                </div>
                <div class="form-group col-md-4 m-b-25" id="lastName">
                    <div class="m-b-10 siteInfoLabel">Last Name</div>
                    <apex:inputText styleClass="form-control required" value="{!sitealtcontactlastname}" label="Last Name" onkeyup="fieldValidationForFirstPage(event,this,'secondaryLastName');" onblur="fieldValidationForFirstPage(event,this,'secondaryLastName');"/>
                </div>
                <div class="form-group col-md-4 m-b-25" id="email">
                    <div class="m-b-10 siteInfoLabel">Email Address</div>
                    <apex:inputText styleClass="form-control required" value="{!sitealtcontactemailaddress}" label="Email Address" onkeyup="fieldValidationForFirstPage(event,this,'secondaryEmail');" onblur="fieldValidationForFirstPage(event,this,'secondaryEmail');"/>
                </div>
            </div>
            <div class="row ">
                <div class="form-group col-md-4 m-b-10" id="phoneNumber">
                    <div class="m-b-10 siteInfoLabel">Business Phone Number</div>
                    <apex:inputText styleClass="form-control required" value="{!sitealtcontactphonenumber}" label="Phone Number" onkeyup="fieldValidationForFirstPage(event,this,'secondaryBussinessPhoneNumber');" onblur="fieldValidationForFirstPage(event,this,'secondaryBussinessPhoneNumber');"/>
                </div>
                <div class="form-group col-md-4 m-b-10" id="mobileNumber">
                    <div class="m-b-10 siteInfoLabel">Mobile Phone Number</div>
                    <apex:inputText styleClass="form-control required" value="{!sitealtcontactmobile}" label="Mobile Phone Number" onkeyup="fieldValidationForFirstPage(event,this,'secondaryMobilePhoneNumber');" onblur="fieldValidationForFirstPage(event,this,'secondaryMobilePhoneNumber');"/>
                </div>
            </div>
        </apex:outputPanel>
        
        <hr class="section-dividerHr" />
        
        <apex:pageBlock >
            <div class="sectionHeader-primary m-b-25">Port Information</div>
            <div class="m-b-25"><span class="sectionHeader-secondary">Type: </span><span>{!porttypeselected}</span></div>  
            <ul class="col-xs-12 p0 m0">
                <li class="col-md-4 p0">          
                    <div class="m-b-25"><span class="sectionHeader-secondary">Speed: </span><span>{!portspeedselected}</span></div>
                </li>           
                <li class="m-r-3P p0">
                    <apex:outputPanel rendered="{!mbcSpeed != null || mbcSpeed != ''}">
                        <div class="m-b-25"><span class="sectionHeader-secondary" style="padding-left:10px;">Minimum Bandwidth Commitment (MBC): </span><span>{!mbcSpeed}</span></div>
                    </apex:outputPanel>   
                </li>
            </ul>              
            <div class="m-b-25"><span class="sectionHeader-secondary">Class of Service: </span><span>{!CosSelected}</span></div>            
            
            <!-- Existing POTS for Testing Router -->
            <apex:outputPanel rendered="{!ismanaged}" styleClass="row col-xs-8 m-b-15">
                <div style="font-family: omnesatt_iiregular;font-weight:bold;">Existing POTS for Testing Router</div>
                <apex:selectRadio value="{!selectedId1}" styleClass="col-md-4">
                    <apex:selectOptions value="{!pickList1}" />
                    <apex:actionSupport event="onchange" reRender="ExistingPOTSText" oncomplete="checkIfSubmitBtnCanBeEnabled();" />
                </apex:selectRadio>
                <apex:outputPanel id="ExistingPOTSText">
                    <apex:inputText value="{!ExistingPOTSText}" styleClass="m-b-15" rendered="{!IF(selectedId1 == 'No',true,false)}" onchange="checkIfSubmitBtnCanBeEnabled();"/>
                </apex:outputPanel>
            </apex:outputPanel>
            
            <!-- New POTS Section-->
            <apex:outputPanel id="POTS_Section" rendered="{!IsMisRouter}">
                <div class="row m-b-10" id="POTS_SectionId">
                    <div class="col-md-8 m-t-10" id="POTSRadio">
                        <div style="font-family: omnes_att_iimedium;line-height:5px"><p>Do you have an existing POTS (Plain Old Telephone Service)</p>for testing the Router?
                            <span class="tooltipTrigger glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" title="Line that will be used for Out of Band Access, providing AT&T a way to access your router during turn up and maintenance of the router."></span></div>
                        <apex:selectRadio value="{!selectedPOTSId}" styleClass="required" style="margin-bottom: -15px;" onchange="fieldValidation(event,this,'radioSelection');emptyPOTSPhnNum(this.value);">
                            <apex:selectOption itemLabel="Yes" itemValue="true"/>
                            <apex:selectOption itemLabel="No" itemValue="false"/>
                        </apex:selectRadio>
                        <div class="row"><div class="col-md-10 siteInfoTxtSty m0 m-t-25 m-b-10" ><p>POTS is required to troubleshoot and test your router.</p><p>If you need to establish POTS, please contact your CAM.</p></div></div>
                    </div>
                    <!-- Sites POTS Phone Number -->
                    
                    <apex:outputPanel styleClass="col-md-4" id="SitesPhnNo">
                        <apex:outputPanel styleCLass="SitesPhnNo" rendered="{!IF(selectedPOTSId == 'true',true,false)}"> 
                            <label>Sites POTS Phone Number</label>
                            <apex:inputText styleclass="form-control required" value="{!POTS_PhoneNo}" html-placeholder="0000000000" onkeyup="fieldValidationForFirstPage(event,this,'POTSPhoneNumber');" onblur="fieldValidationForFirstPage(event,this,'POTSPhoneNumber');"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <span class="noteData col-xs-12 p0" >Landline number only</span>
                        </apex:outputPanel>
                    </apex:outputPanel>
                </div>
            </apex:outputPanel> 
            
            <!-- AT&T Hosted DNS -->
            <div class="row m-b-10">
                <div class="col-md-8" id="hostedDNS">
                    <div style="font-family: omnesatt_iiregular;font-weight:bold;">Would you like AT&amp;T to host your DNS?</div>
                    <apex:selectRadio value="{!ATTHostedDNSSeleted}" styleClass="col-md-4" onchange="fieldValidation(event,this,'radioSelection');emptyDomainURL(this.value);">
                        <apex:selectOptions value="{!pickList1}" />
                    </apex:selectRadio>
                </div>
                <!-- Domain URL -->
                <apex:outputPanel styleClass="col-md-4" id="DomainURL">
                    <apex:outputPanel styleClass="DomainURL" rendered="{!IF(ATTHostedDNSSeleted == 'Yes',true,false)}">
                        <label>Domain URL</label>
                        <apex:inputText value="{!DomainURL}" styleClass="form-control required" onkeyup="fieldValidationForFirstPage(event,this,'DomainURL');" onblur="fieldValidationForFirstPage(event,this,'DomainURL');"/>
                    </apex:outputPanel>
                </apex:outputPanel>
            </div>
            
            <ul class="addressFields col-xs-12 p0 m-b-25">
                <apex:outputPanel rendered="{!showInterfaceType}">
                  <li class="m-r-3P col-md-4 p0" style="width:32%;"> 
                      <label>Interface Type</label> <span class="tooltipTrigger glyphicon glyphicon-question-sign intractiveIcon" data-toggle="tooltip" data-html="true" data-placement="right" 
                                                          data-original-title="<div style='width:274px;font-family:omnes_att_iimedium;font-size:12px;'>Distance Limitations on 100BaseT = 100 Meters. (So a 50M service more than 100 Meters from the Demarcation must be Optical.)"></span>
                        <apex:outputPanel rendered="{!enableInterfaceType}">
                            <apex:selectList value="{!interface_Type}" styleClass="form-control styled-select-small adjust-select-small interfaceType"  onblur="fieldValidation(event,this,'dropDownSelection');" onchange="fieldValidation(event,this,'dropDownSelection');" multiselect="false" size="1">
                                <apex:selectOptions value="{!interfaceTypeOptionList}" /> 
                            </apex:selectList>
                        </apex:outputPanel>
                        <apex:outputPanel rendered="{!!enableInterfaceType}">
                            <apex:selectList value="{!interface_Type}" styleClass="form-control disabled-styled-select-small interfaceType" disabled="true" multiselect="false" size="1">
                                <apex:selectOptions value="{!interfaceTypeOptionList}" /> 
                            </apex:selectList>
                        </apex:outputPanel>
                      <span class="noteData interfacetypeNote col-xs-12 p0">Option from Configuration page can be changed.</span>
                  </li>
                </apex:outputPanel> 
                <li class="m-r-3P col-md-4 p0" > 
                    <label>Jack Type</label> 
                    <apex:selectList value="{!jack_Type}" styleClass="form-control jackTypeDropDowns styled-select-small adjust-select-small required" onblur="fieldValidation(event,this,'dropDownSelection');" onchange="fieldValidation(event,this,'dropDownSelection');" multiselect="false" size="1" style="-moz-appearance: none;-webkit-appearance: none;font-size:16px;"> 
                        <apex:selectOptions value="{!jackTypeOptionList}" /> 
                    </apex:selectList>
                </li> 
            </ul> 
            
            <label class="dualStackNote"><b>Note: Dual Stack is the recommended IP Type.</b> Dual Stack = Unnumbered IPv4 (1 LAN, 0 WAN); IPv6 (/56 LAN and /64 WAN)</label>
            <!-- IP Type -->
            <div class="row IPType">
                <div class="col-md-12 ">
                    <label>IP Type </label> <span class="tooltipTrigger glyphicon glyphicon-question-sign intractiveIcon" data-toggle="tooltip" data-placement="right" title="Dual Stack supports both IPv4 and IPv6."></span>
                    <apex:selectRadio id="IPType" value="{!selectedId2}" styleClass="required">
                        <apex:selectOptions value="{!IpTypePicklistOptions}" />
                        <apex:actionSupport event="onchange" action="{!renderipv4blocks}" reRender="ipv4CriteriaBlock,ipv4block,ipv6block" oncomplete="removeErrorMsg('IPType');" status="loading" />
                    </apex:selectRadio>                   
                </div>
            </div>
            <!--IP4 Criteria -->
            <apex:outputPanel id="ipv4CriteriaBlock">
                <apex:outputPanel rendered="{!IF(selectedId2 == 'IPv4',true,false)}">
                  <div class='ipv4CriteriaBox'>
                      <div class="criteriaHeader"><b>Select the criteria that applies to your needs to proceed with the RDS.</b></div>
                      <apex:selectRadio value="{!ipV4_Exception}" styleClass="required ipv4CriteriaOptions">
                          <apex:selectOptions value="{!ipV4CriteriaOptionsList}" />
                          <apex:actionSupport event="onchange" action="{!ipv4ExceptionSelected}" reRender="ipv4block" oncomplete="removeErrorMsg('IPV4CriteriaRadioSelection');" status="loading" />
                      </apex:selectRadio>
                  </div>
                  <apex:outputPanel rendered="{!IF(ipV4_Exception == '',true,false)}">
                        <div class="m-t-10 errorText">Selection required</div>
                  </apex:outputPanel>
                </apex:outputPanel>
            </apex:outputPanel>
            
            <!--IPV4 INFORMATION STARTS HERE -->
            <apex:outputPanel id="ipv4block">
                <apex:outputPanel rendered="{!IF((ipV4_Exception != '' || selectedId2 == 'Dual Stack'),true,false)}">
                    <section id="section1Id">
                        <hr class="section-dividerHr" style="margin-top:20px;"/>
                        <h3>IPv4 Information</h3>
                        <div class="row" style="margin-top: 20px;">
                            <div class="col-xs-12">
                                <apex:outputPanel id="LANBlockDisplay1">
                                    <div class="dataTables_wrapper">
                                        <table class="lan-adr-type table table-condensed table-striped table-bordered">
                                            <tr>
                                                <th width="25%"><label>LAN Address Type</label></th>
                                                <th width="30%"><label>Public IP Address</label></th>
                                                <th width="30%"><label style="margin-right:0px !important;">LAN IP Length</label><span style="margin-left:10px !important;" class="tooltipTrigger glyphicon glyphicon-question-sign" data-toggle="tooltip" data-placement="right" 
                                                                                                       data-html="true" data-original-title="<div style='width:150px;font-size:12px;'>The standard default IP Length is /29"></span></th>
                                             </tr>
                                            <apex:repeat value="{!LANSBlocksWrapper1}" var="lan">
                                                <tr>
                                                    <td>            
                                                        <h4>{!DefaultCustomer}</h4>
                                                    </td>    
                                                    <td>            
                                                        <apex:inputText styleClass="form-control" disabled="true" html-placeholder="N/A" />
                                                    </td>
                                                    <td>
                                                        <apex:selectList id="IPLength" value="{!lanlengthV4}" size="1" multiselect="false" styleClass="form-control styled-select-small">
                                                                <apex:selectOptions value="{!IpLengthPicklistOptions}" />
                                                            </apex:selectList>                                                          
                                                    </td>
                                                </tr>
                                            </apex:repeat>
                                        </table>
                                    </div>
                                </apex:outputPanel>
                            </div>
                        </div>
                        <div class="row top-m-25">
                            <!-- Routing Protocol -->
                            <div class="col-md-4">
                                <apex:outputLabel value="Routing Protocol" />
                                <apex:selectRadio styleClass="col-xs-8" value="{!DefaultBGP}">
                                    <apex:selectOption itemLabel="Static" itemValue="STATICROUTING"></apex:selectOption>
                                    <apex:selectOption itemLabel="BGP" itemValue="BGP4"></apex:selectOption>
                                    <apex:actionSupport event="onchange" action="{!ResetBGPStaticValues4}" reRender="ASNProvider,ASNNumber,BGPdisplay,CIRData,CIRData2" oncomplete="checkIfSubmitBtnCanBeEnabled();reInitiateToolTip();" status="loading"/>
                                </apex:selectRadio>
                            </div>
                            <!-- ASN Provider -->
                            <div class="col-md-4 ASNProvider">
                                <apex:outputPanel id="ASNProvider">
                                    <apex:outputPanel rendered="{!IF(DefaultBGP == 'BGP4',true,false)}">
                                        <apex:outputLabel value="Who provides your ASN?" />
                                        <apex:selectRadio value="{!whoProvidesASNv4}">
                                            <apex:selectOption itemLabel="AT&T" itemValue="ATT"></apex:selectOption>
                                            <apex:selectOption itemLabel="Public Registered" itemValue="Public Registered"></apex:selectOption>
                                            <apex:actionSupport event="onchange" action="{!regenerateASNNumberv4}" reRender="ASNNumber" oncomplete="checkIfSubmitBtnCanBeEnabled();" status="loading"/>
                                        </apex:selectRadio>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </div>  
                            <!-- Autonomous System Number, only displays if BGP is selected -->
                            <div class="col-md-4">
                                <apex:outputPanel id="ASNNumber">
                                    <apex:outputPanel rendered="{!IF((DefaultBGP == 'BGP4' && whoProvidesASNv4 != ''),true,false)}">
                                        <apex:outputLabel value="Autonomous System Number" />
                                        <apex:inputText value="{!AutonomousSystemNumber}" disabled="{!IF(whoProvidesASNv4 == 'ATT',true,false)}" styleClass="form-control {!IF(whoProvidesASNv4 != 'ATT','required','')}" onkeyup="fieldValidationForFirstPage(event,this,'ASNForIP');" onblur="fieldValidationForFirstPage(event,this,'ASNForIP');"/>
                                        <apex:outputText rendered="{!IF(whoProvidesASNv4 != 'ATT',true,false)}"><span class="col-md-12 p0 noteData">Minimum 4 digits. Maximum 6 digits.</span></apex:outputText>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </div>
                        </div>
                    </section>
                    
                    <!-- Begin BGP Display -->
                    <apex:outputPanel id="BGPdisplay">
                        <apex:outputPanel rendered="{!IF(DefaultBGP == 'BGP4',true,false)}">
                            <!-- Routing Row -->
                            <div class="row">
                                <!-- Default Routing -->
                                <div class="col-md-4">
                                    <apex:outputLabel value="Default Route" />
                                    <apex:selectRadio styleClass="col-xs-8" value="{!RestrictiveRouting4}">
                                        <apex:selectOption itemLabel="Yes" itemValue="true"></apex:selectOption>
                                        <apex:selectOption itemLabel="No" itemValue="false"></apex:selectOption>
                                        <apex:actionSupport event="onchange" reRender="RouteAdvertisementPanel" oncomplete="checkIfSubmitBtnCanBeEnabled();reInitiateToolTip();" status="loading"/>
                                    </apex:selectRadio>
                                </div>
                                <!-- Preferred Routing Option -->
                                <div class="col-md-4">
                                    <apex:outputPanel id="RouteAdvertisementPanel">
                                        <apex:outputPanel rendered="{!IF(RestrictiveRouting4 == 'false',true,false)}" styleClass="container-full">
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <label>Preferred Route Option</label>
                                                    <span class="tooltipTrigger glyphicon glyphicon-question-sign intractiveIcon" data-toggle="tooltip" data-html="true" data-placement="right" 
                                                          data-original-title="<div style='width:315px;font-family:omnes_att_iimedium;font-size:12px;'><b>AT&T Routes</b> includes Candidate Default Networks.<br><b>Full Internet Routes</b> includes AT&T Routes and
                                                        Candidate Default Networks.<br>In general, the more BGP routes, the larger the memory the router must have.
                                                        Check with the router vendor in order to determine the exact router configuration required to support the chosen BGP routing configuration."></span>
                                                
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-xs-12">
                                                    <apex:selectList styleClass="form-control required styled-select-small" multiselect="false" size="1" value="{!Routevalue4}" style="font-size:16px;" onblur="fieldValidation(event,this,'dropDownSelection');" onchange="fieldValidation(event,this,'dropDownSelection');">
                                                        <apex:selectOptions value="{!RouteTypeOptions}" />
                                                    </apex:selectList>
                                                </div>
                                            </div>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                </div>
