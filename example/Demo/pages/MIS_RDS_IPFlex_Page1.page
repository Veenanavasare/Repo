                    <apex:selectRadio styleClass="col-xs-4" value="{!EarlyServiceTurnuponDueDate}" label="Decline Early Service and Provision on Due Date?">
                        <apex:selectOption itemLabel="Yes" itemValue="true" />
                        <apex:selectOption itemLabel="No" itemValue="false" />
                    </apex:selectRadio>
                </div>
            </div>                
        </div>
        
        <hr class="section-dividerHr" style="margin-bottom: 20px;"/>
        
        <div class="conatiner-full col-md-12 p0" style="float:left;">
            <h3>Additional Information</h3>
            <div class="row">
                <div class="col-xs-12 additionalInfoSection">
                    <apex:outputPanel rendered="{!IF(isIPFlexEnabled == false,true,false)}">
                        <div style="margin:10px 0;">Tell us anything else we need to know about this site. (eg: Please call Local Contact 24 hours prior to dispatch.)(Optional)</div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!IF(isIPFlexEnabled == true,true,false)}">
                        <div style="margin:10px 0;">Tell us anything else we need to know about this site such as New Construction, New Block of Local DID number, any other requirements for your site. (Optional)</div>
                    </apex:outputPanel>
                    <apex:input id="myTextarea" value="{!siteadditionalinfo}" html-maxlength="68" styleClass="w100p form-control myTextareaClass" style="margin-bottom: 10px;" html-placeholder="No special characters allowed. Max 68 characters." onkeyup="CharLimit(this, '68');fieldValidationForFirstPage(event,this,'FeedBackField');"/>
