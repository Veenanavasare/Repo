                                <div class="col-xs-12">
                                    <div id="cosSlider" style="height:32px;">
                                        <div class="cosSliderLabelsProgress">
                                            <span id="cosSliderPrgs">{!SelectedSliderValue}%</span>
                                        </div>
                                        <div class="cosSliderLabelsMax">
                                            <span id="cosSliderMax">100%</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="row">
                                <span class="col-xs-1 axisPlacementStyle" >|</span>
                                <span class="col-xs-10 axisPlacementStyle" style="text-align:center;">|</span>
                                <span class="col-xs-1 axisPlacementStyle" style="text-align:right;">|</span>                                        
                            </div>
                            
                            <div class="row">
                                <span style="padding-left:12px;font-weight:bold;" class="col-xs-1 axisPlacementStyle">0</span>
                                <span class="col-xs-10 axisPlacementStyle" style="text-align:center;font-weight:bold;">50</span>
                                <span class="col-xs-1 axisPlacementStyle" style="padding-left:36px;font-weight:bold;">100</span>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Step 2: Select Profile ID  -->                            
                <div class="row">
                    <div class="col-xs-12" >
