        return this.replace(/^\s+/,"");
    }

            var j$ = jQuery.noConflict();
            j$(document).ready(function () {
                j$(".tooltipTrigger").tooltip();
                initializeFieldValidations();
        });
        // Function to initialize each of the validations present on the form fields
        function initializeFieldValidations() {
            // Add a keyup event handler to each of the fields to ensure that on keyup we validate the field
            // Solution Provider Point of Contact
            // Primary First Name
            j$(".inputPriFirstName").on({"keyup": function()
                {
                    validateEmptyFields("inputPriFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                }
            });
            // Primary Last Name
            j$(".inputPriLastName").on({"keyup": function()
                {
                    validateEmptyFields("inputPriLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                }
            });
            // Primary Email Address
            j$(".inputPriEmail").on({"keyup": function()
                {
                    validateEmptyFields("inputPriEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                }
            });
            // Primary Address 1
            j$(".inputPriAddress1").on({"keyup": function()
                {
                    validateEmptyFields("inputPriAddress1","Address 1 should not begin with a space","Address 1 cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriAddress1","Address 1 should not begin with a space","Address 1 cannot be blank", true);
                }
            });
            // City
            j$(".inputPriCity").on({"keyup": function()
                {
                    validateEmptyFields("inputPriCity","City should not begin with a space","City cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriCity","City should not begin with a space","City cannot be blank", true);
                }
            });

            // Numerical Fields - Verify the length along with the checks for invalid characters and empty space
            // Primary Phone Number
            j$(".inputPriPhone").on({"keyup": function()
                {
                    validateNumericalFields(j$(".inputPriPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputPriPhone","Phone number should not begin with a space");
                },
            "blur":function()
                {
                    validateNumericalFields(j$(".inputPriPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputPriPhone","Phone number should not begin with a space");
                }
            });
            // Primary Mobile Number
            j$(".inputPriMobile").on({"keyup": function()
                {
                    validateNumericalFields(j$(".inputPriMobile").val(), "Invalid Mobile Number", "Mobile Number cannot be blank", "Mobile Number must be ten digits", false,"inputPriMobile","Mobile Number should not begin with a space");
                },
            "blur":function()
                {
                    validateNumericalFields(j$(".inputPriMobile").val(), "Invalid Mobile Number", "Mobile Number cannot be blank", "Mobile Number must be ten digits", false,"inputPriMobile","Mobile Number should not begin with a space");
                }
            });
            // Primary Zip Code
            j$(".inputPriZipCode").on({"keyup": function()
                {
                    validateNumericalFields(j$(".inputPriZipCode").val(), "Invalid Zip Code", "Zip Code cannot be blank", "Zip Code must be five digits", true, "inputPriZipCode","Zip Code should not begin with a space");
                },
            "blur":function()
                {
                    validateNumericalFields(j$(".inputPriZipCode").val(), "Invalid Zip Code", "Zip Code cannot be blank", "Zip Code must be five digits", true, "inputPriZipCode","Zip Code should not begin with a space");
                }
            });
            // End Solution Provider Point of Contact

            // Billing Contact
            // Alternate First Name
            j$(".inputAltFirstName").on({"keyup": function()
                {
                    validateEmptyFields("inputAltFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputAltFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                }
            });
            // Alternate Last Name
            j$(".inputAltLastName").on({"keyup": function()
                {
                    validateEmptyFields("inputAltLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputAltLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                }
            });
            // Alternate Email Address
            j$(".inputAltEmail").on({"keyup": function()
                {
                    validateEmptyFields("inputAltEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputAltEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                }
            });
            
            // Numerical Fields - Verify the length along with the checks for invalid characters and empty space
            // Alternate Phone Number
            j$(".inputAltPhone").on({"keyup": function()
                {
                    validateNumericalFields(j$(".inputAltPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputAltPhone","Phone number should not begin with a space");
                },
            "blur":function()
                {
                    validateNumericalFields(j$(".inputAltPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputAltPhone","Phone number should not begin with a space");
                }
            });
            // End Billing Contact

            // Primary Contact
            // Primary Contact First Name
            j$(".inputPriContactFirstName").on({"keyup": function()
                {
                    validateEmptyFields("inputPriContactFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriContactFirstName","First Name should not begin with a space","First Name cannot be blank", true);
                }
            });
            // Primary Contact Last Name
            j$(".inputPriContactLastName").on({"keyup": function()
                {
                    validateEmptyFields("inputPriContactLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriContactLastName","Last Name should not begin with a space","Last Name cannot be blank", true);
                }
            });
            // Primary Contact Email Address
            j$(".inputPriContactEmail").on({"keyup": function()
                {
                    validateEmptyFields("inputPriContactEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputPriContactEmail","Email Address should not begin with a space","Email Address cannot be blank", true);
                }
            });
            // Primary Contact Phone Number
            j$(".inputPriContactPhone").on({"keyup": function()
                {
                    validateNumericalFields(j$(".inputPriContactPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputPriContactPhone","Phone number should not begin with a space");
                },
            "blur":function()
                {
                    validateNumericalFields(j$(".inputPriContactPhone").val(), "Invalid Phone number", "Phone number cannot be blank", "Phone number must be ten digits", true, "inputPriContactPhone","Phone number should not begin with a space");
                }
            });
            // End Primary Contact
            // Installation Details
            // Local Business Name
            j$(".inputLocalBusinessName").on({"keyup": function()
                {
                    validateEmptyFields("inputLocalBusinessName","Business Name should not begin with a space","Business Name cannot be blank", true);
                },
            "blur":function()
                {
                    validateEmptyFields("inputLocalBusinessName","Business Name should not begin with a space","Business Name cannot be blank", true);
                }
