<style type="text/css">
    .tooltipTrigger{cursor: pointer;}
    
    /* Styles added for the error messages to appear correctly */
    .form-group
    {
        height: 100px;
    }
    /* Error message class */
    .errorText
    {
        color: #a94442;
        font-size: 18px;
    }
    /* Overwrite bootstrap */
    .btn-default.disabled
    {
        color: white;
        background-color: #056fab;
    }
    .btn-complete-parent-disabled
    {
        cursor: not-allowed;
    }
    #manage_content label.radio-inline
    {
        margin: 10px 5px;
    }
    .tooltipTrigger
    {
        margin-top: 10px;
    }
    .blue_hr
    {
        width: 100%;
    }
    .darkDiv {
        background: #E6E6E6;
        margin: 10px 0;
        padding: 10px 20px;
        font-family: omnesatt_iiregular;
    }
    .col-md-3 .progress
    {
        margin: 15px auto 5px;
    }
    .progress_full, .progress_2, .progress_1 {
        position: relative;
        margin: 10px auto 5px;
        width: 82px;
        height: 11px;
        background-size: 82px 44px;
        background-image: url("{!URLFOR($Resource.portalDesignImg, 'meter.png')}");
