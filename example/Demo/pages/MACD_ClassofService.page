    <script>
        function vlanExpandIPInfoPanel(vlanId){
            enableThisVlanPanel(vlanId);
        }
        
        function vlanExpandConfigurationPanel(vlanId){
            document.getElementById("hdnSelectedVlanId").value = vlanId;           
            enableAndShowConfigPanel(vlanId);  
        }
        
        function undoChangesJS(vlanId){
        
            invokeUndoChangesAjax(vlanId);
        }
        function resetAndCancel(){
        
            doProcessResetAndCancel();
        }
        function cancelallchangesAndExit(){
        
                cancelAndExitJs();
        }
        function navigateToDashBoard(){
        
            window.location.href = '/portaldashboard_ui';
        }
        

    </script>
        <input type="hidden" id="hdnSelectedVlanId" value="" />
        <apex:form id="parentForm" >
            <input type="hidden" id="hdnSelectedSliderValue" value="{!existingSliderValue}" />

            <apex:actionFunction name="initCoSChangeVlan" reRender="parentForm,rptVlanPanel,vlanIpInfoPanel" status="loading" action="{!doDefaultLoad}" />
            
            <apex:actionFunction name="doProcessResetAndCancel" reRender="" status="loading" action="{!resetAndCancelTheQuote}" oncomplete="navigateToDashBoard();">
                
            </apex:actionFunction>
            <apex:actionFunction name="enableThisVlanPanel" reRender="parentForm,rptVlanPanel,vlanIpInfoPanel" status="loading" action="{!enableAndExpandTheSelectedPanel}">
                <apex:param assignTo="{!selectedVlanId}" name="vlanId" value="" />
            </apex:actionFunction>
            <apex:actionFunction name="enableAndShowConfigPanel" reRender="parentForm,rptVlanPanel,vlanIpInfoPanel" status="loading" action="{!enableAndExpandTheSelectedConfigPanel}" oncomplete="updateSlider();">
                <apex:param assignTo="{!selectedVlanId}" name="vlanId" value="" />
            </apex:actionFunction>
            <apex:actionFunction name="invokeUndoChangesAjax" reRender="parentForm,rptVlanPanel,vlanIpInfoPanel,reviewPanelId" status="loading" action="{!doUndoChanges}" oncomplete="setTimeout(function(){ updateSlider(); }, 1000);" >
                <apex:param assignTo="{!selectedVlanId}" name="vlanId" value="" />
            </apex:actionFunction>
            <apex:actionFunction name="getCosProfiles" action="{!fetchProfileCosValues}" oncomplete="" reRender="cosProfileID,btnApplyAndSave" status="loading">
                <apex:param name="slidervalue" value="" assignTo="{!selectedSliderValue}"/>
            </apex:actionFunction>
            <apex:actionFunction name="doUpdateValidation" action="{!doUpdateOnValidationRecord}" rerender="parentForm,rptVlanPanel,vlanIpInfoPanel,reviewPanelId,confirmPanelId" status="loading" oncomplete="enableThankyouPanel();"/>
            
            <apex:outputPanel id="parentPanelVLanInfo" rendered="{!!isSubmitted}">
                <apex:variable var="cntr" value="{!0}"/>
                <apex:repeat id="rptVlanPanel" value="{!listVlanDataWrapper}" var="vlanData">
                <apex:variable var="cntr" value="{!cntr+1}"/>
                <apex:variable var="count" value="{!0}"/>
                <apex:variable var="count" value="{!count+1}"/> 
