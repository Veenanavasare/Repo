        function validateIPLength(sClassName, sErrorMessage, sBlankMessage, boolIsRequired) {
            validateEmptyFields(sClassName, sErrorMessage, sBlankMessage, boolIsRequired);
            console.log(sClassName);
            var sValue = "",
                oField = j$("." + sClassName),
                oParent = oField.parent(),
                isValid, messageValue;
            if (oParent[0] && !oParent.hasClass("has-error")) {
                sValue = oField.val();
                if (isNaN(sValue)) {
                    messageValue = 'Invalid input!';
                } else if ( !(sValue > 48 && sValue < 129)) {
                    messageValue = 'Invalid Length';
                } else if (sClassName == 'MISV4_ATT' && !(sValue > 25 && sValue < 32)) {
                    messageValue = invalidInput_IPLength;
                } else if (MISV4_Cust && !(sValue > 25 && sValue < 32)) {
                    messageValue = invalidInput_IPLength;
                } else {
                    // Remove the error message
                    isValid = true;
                }
                if (isValid) {
                    oParent.removeClass("has-error");
                    // Remove all spaces and () from the string
                    j$(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                } else {
                    j$(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                    oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>" + messageValue + "</p>");
