    <script> 
        function initializeIPValidationFor(sClassName) {
            var comp = j$("." + sClassName);
            comp.on({
                "keyup": function () {
                    validateIP(sClassName);
                    checkForErrorsPage2();
                },
                "blur": function () {
                    validateIP(sClassName);
                    checkForErrorsPage2();
                }
            });
        }

        function initializeCustomIPValidationFor(sClassName) {
            var comp = j$("." + sClassName);
            comp.on({
                "focus": function () {
                    setValidValues(sClassName);
                },
                "keyup": function () {
                    validateCustomIP(sClassName, 'IP range should not begin with a space', 'IP range cannot be blank', true);
                    checkForErrorsPage2();
                },
                "blur": function () {
                    validateCustomIP(sClassName, 'IP range should not begin with a space', 'IP range cannot be blank', true);
                    checkForErrorsPage2();
                }
            });
        }
        
         function disableButton(sClassName, disable) {
            
           //var isProfileValid = document.getElementById("hdnIsProfileVaalid").value;
            //var submitElem =    document.getElementsByClassName(sClassName);
            //alert(disable+'--------------'+submitElem+'--------------'+isProfileValid);
            sClassName = '.' + sClassName;
            if (disable) {
                j$(sClassName).addClass('disabled');
                j$(sClassName).prop('disabled', true);
            } else {
                j$(sClassName).removeClass('disabled');
                j$(sClassName).removeAttr('disabled');
            }
        }
        
        function validatePage2Submit() {
            console.log('validatePage2Submit');
            var arrRequiredFields = j$(".requiredInputField"),
                boolIsNumberValid = true,
                boolIsProfilesValid = true,
                boolIsHubSelected = true,
                boolIsValid = true;
            if(arrRequiredFields.length > 1){
                arrRequiredFields.each(function () {
                    var oRequiredField = j$(this);
                    if (oRequiredField[0]) {
                        if (0 == oRequiredField.val().length) {
                            boolIsValid = false;
                            return false;
                        }
                    }
                });
                console.log('requiredSelectField.length: ' + boolIsProfilesValid +' boolIsHubSelected: '+boolIsHubSelected);
                // If there is any error message present, do not enable the button
                if (0 < j$(".has-error").length) {
                    boolIsNumberValid = false;
                }
                if(0 < j$(".requiredInputFieldNumber_error").length){
                    boolIsValid =false;
                }
                if(0 < j$(".requiredInputFieldNumberipv6_error").length){
                    boolIsValid =false;
                }
                disableButton('btnContinuePage2',!(boolIsValid));
            }
            else{
                disableButton('btnContinuePage2',true);
            }
            if(ipFlag){
             checkFinalFlag();
            }
            validateMD5Password();
            validateMD5PasswordV6()
            
        }
        
        function setValidated(args){
        
            alert('IN');
            document.getElementById("hdnIsProfileVaalid").value = args;
            validatePage2Submit();
        }
    
        
    </script>
