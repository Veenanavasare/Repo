    function ApplyChangesJS(vlanId){
        
        $.confirm({

        closeIcon: true,
            text: "<div style='text-align:center;font-weight:bold;margin-top:18px'>Applying the configuration to all VLANs will overwrite any <br/> previously configured CoS for VLANs on this site.<br/><br/> Do you want to apply the configuration to all VLANs on this site?<br/></div>",
            confirm: function(button) {
                
                invokeApplyChangesAjaxForAdd(vlanId);   
            },
            cancel: function(button) {
                
            },
            confirmButton: "Yes,apply to all",
            cancelButton: "No"
        });
        
    }
    function resetAndCancel(){
        
        doProcessResetAndCancelForAdd();
    }
    function navigateToDashBoard(){
        
        window.location.href = '/portaldashboard_ui';
    }
    
