        function findMaxDate() {
            var today = new Date();
            var weekends = [0, 6];
            var maximumDays = maxDateValue;
            var i = 0;
            while (i < maximumDays) {
                today.setDate(today.getDate() + 1);
                var todayWeekday = today.getDay();
                if (jQuery.inArray(todayWeekday, weekends) < 0 && isHoliday(today))
                    i++;
            }
            return today;
        }
