        function GetURLParameter(sParam) {
            var sPageURL = window.location.search.substring(1);
            var sURLVariables = sPageURL.split('&');
            for (var i = 0; i < sURLVariables.length; i++) {
                var sParameterName = sURLVariables[i].split('=');
                if (sParameterName[0] == sParam) {
                    return sParameterName[1];
                }
            }
        }    
        
        function progressBar(percent,available, $element,pendingvalue) {
            percent=Math.round(percent);
            var percent1=percent-pendingvalue;
            var pSpeed = "{!listDisplayValidationRecords.Port_Speed__c}";
            var progressBarWidth = percent1 * $element.width() / pSpeed;
            $('#addvlanprogbardiv1').animate({width:progressBarWidth}, pSpeed).html();
            var progressBarpendingWidth = pendingvalue * $element.width() / pSpeed;
            $('#addvlanprogbardiv2').animate({width:progressBarpendingWidth}, pSpeed).html();
            var progressBarWidthSpan = (pSpeed-percent) * $element.width() / pSpeed - 1;
            $element.find('span').animate({ width:progressBarWidthSpan }, pSpeed).html();
            if((pSpeed-percent)==0){
                document.getElementById("avlblId").style.display="none";
            }else{
                document.getElementById("avlblId").style.display="block";
            }
        }
