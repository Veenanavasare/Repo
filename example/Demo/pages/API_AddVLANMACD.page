        function validateMD5PasswordField(pValue,errMsg){
            if ("" === pValue) {
                errMsg.innerHTML = 'Password cannot be blank';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
            }else if(pValue.length<8){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
            }else if(pValue.search(/[A-Z]/) < 0){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
            }else if (pValue.search(/[0-9]/) < 0) {
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv4conf]').prop('disabled', true);
            }else{
                errMsg.innerHTML = '';
                jQuery('[id$=pwdv4conf]').prop('disabled', false);
            }
        }
        
        function validateMD5PasswordFieldV6(sValue,errMsg){
            if ("" === sValue) {
                errMsg.innerHTML = 'Password cannot be blank';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
            }else if(sValue.length<8){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
            }else if(sValue.search(/[A-Z]/) < 0){
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
            }else if (sValue.search(/[0-9]/) < 0) {
                errMsg.innerHTML = 'Password must be at least 8 characters, no more than 16 characters and contain at least 1 number and 1 uppercase letter.';
                jQuery('[id$=pwdv6conf]').prop('disabled', true);
            }else{
                errMsg.innerHTML = '';
                jQuery('[id$=pwdv6conf]').prop('disabled', false);
            }
        }
        
