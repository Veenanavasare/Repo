        function initializeFieldValidationsPage2() {
            // Dropdowns
            // VLAN Name
            $(".inputVPNName").on("change", function () {
                validateTextFieldsPage2("inputVPNName", "Please enter VPN Name", "Please enter VPN Name", true);
            });
            
            // IP Type
            $(".inputIPType").on("change", function () {
                handleresetbuttontoggle();
                validateTextFieldsPage2("inputIPType", "Please select a value for IP Type", "Please select a value for IP Type", true);
            });
            
            $(".inputRouteAd").on("change", function () {
                validateTextFieldsPage2("inputRouteAd", "Please Select Route Advertisement", "Please Select Route Advertisement", true);
            });
            
            $(".inputRouteAd1").on("change", function () {
                validateTextFieldsPage2("inputRouteAd1", "Please Select Route Advertisement", "Please Select Route Advertisement", true);
            });
            
            // input fields
            $(".requiredInputFieldNumber").on({
                "keyup": function () {
                    validateTextFieldsPage2("requiredInputFieldNumber", "Autonomous System Number cannot begin with a space", "Please select a value for Autonomous System Number", true)
                },
                "blur": function () {
                    validateTextFieldsPage2("requiredInputFieldNumber", "Autonomous System Number cannot begin with a space", "Please select a value for Autonomous System Number", true)
                }
            });
            
            $(".requiredInputFieldNumberipv6").on({
                "keyup": function () {
                    validateTextFieldsPage2("requiredInputFieldNumberipv6", "Autonomous System Number cannot begin with a space", "Please select a value for Autonomous System Number", true)
                },
