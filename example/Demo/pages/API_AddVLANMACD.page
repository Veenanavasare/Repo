        function checkForErrorsPage2() {
            $('.btnSave, .btnContinue').addClass('disabled');
            // Iterate through all fields and check if there are any invalid entries. If yes, then return false    
            var arrRequiredFields = $(".requiredInputField"),
                boolIsNumberValid = true,
                boolIsValid = true,
                oAddCustomerButton = $('.btnContinuePage2');
            arrRequiredFields.each(function () {
                oRequiredField = $(this);
                if (oRequiredField[0]) {
                    if (0 === oRequiredField.val().length) {
                        boolIsValid = false;
                        return false;
                    }
                }
            });
            
            var flagex=false;
            // If there is any error message present, do not enable the button
            var errorflds=$(".errorText");
            for(var i=0;i<errorflds.length;i++){
                if((errorflds[i].innerHTML).indexOf("Premier SSO failed")>-1)
                {
                    flagex=true;
                    break;
                }
            }
            
            if(flagex){
                if (1 < $(".errorText").length) {
                    boolIsNumberValid = false;
                }
            }else{
                if (0 < $(".errorText").length) {
                    boolIsNumberValid = false;
                }
            }
            
            if (boolIsValid && boolIsNumberValid) {
                // Add customer button enabled
                if (oAddCustomerButton[0]) {
                    oAddCustomerButton.parent().removeClass("btn-complete-parent-disabled");
                    oAddCustomerButton.removeClass("disabled");
                }
            } else {
                // Add customer button disabled
                if (oAddCustomerButton[0]) {
                    oAddCustomerButton.parent().addClass("btn-complete-parent-disabled");
                    oAddCustomerButton.addClass("disabled");
                }
            }
            
            //for slider start
            if($('.speedselectedaddvlan')!=null)
            {
                var valuespeed = $('.speedselectedaddvlan').html();
                if(valuespeed=="" || valuespeed==null || valuespeed=="0" || valuespeed==" "){
                    if (oAddCustomerButton[0]) {
                        oAddCustomerButton.parent().addClass("btn-complete-parent-disabled");
                        oAddCustomerButton.addClass("disabled");
                    }
                }
            }
            handleresetbuttontoggle();
        }
        
        function validateTextFieldsPage2(sClassName, sErrorMessage, sBlankMessage, boolIsRequired) {
            var sValue = "",
                oField = $("." + sClassName),
                oParent = oField.parent();
            if (oField[0]) {
                // Check if the field contains blank value as the first character
                sValue = oField.val();
                // Handle dropdown value as well
                if ((boolIsRequired) && ("" === sValue || "None(ARPA)" === sValue)) {
                    // Append the error message in case the field is not filled
                    if (oParent[0]) {
                        // Remove all spaces and () from the string
                        $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                        oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>" + sBlankMessage + "</p>");
                    }
                } else if ((" " === sValue) || (sValue !== sValue.trimLeft())) {
                    // Append the error message in case the field is wrongly filled
                    if (oParent[0]) {
                        // Remove all spaces and () from the string
                        $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                        oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>" + sErrorMessage + "</p>");
                    }
                }
                	// Check if this is a valid number
                    else if (("requiredInputFieldNumber" === sClassName || "requiredInputFieldNumberipv6" === sClassName) && isNaN(sValue)) {
                        if (oParent[0]) {
                            // Remove all spaces and () from the string
                            $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                            oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>Invalid Entry</p>");
                        }
                    }
                	// Number fields - ASN Number
                        else if (("requiredInputFieldNumber" === sClassName || "requiredInputFieldNumberipv6" === sClassName) && (parseInt(sValue, 10) < 64512 || parseInt(sValue, 10) > 64999) && (parseInt(sValue, 10) < 65005 || parseInt(sValue, 10) > 65534)) {
                            // Append the error message in case the field is wrongly filled
                            if (oParent[0]) {
                                // Remove all spaces and () from the string
                                $(".errorText." + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error").remove();
                                oParent.addClass("has-error").append("<p class='errorText " + sClassName.replace(/\s/g, "_").replace(/["'()]/g, "") + "_error'>Invalid Entry</p>");
                            }
                        } else {
                            // Remove the error message
                            if (oParent[0]) {
                                oParent.removeClass("has-error");
                                // Remove all spaces and () from the string
                                $(".errorText." + sClassName.replace(/\s/g, "_").replace(/[""()]/g, "") + "_error").remove();
                            }
                        }
            }
            // All Field Check
            checkForErrorsPage2();
        }
        
        function resetAllFields(){
            $(".inputVPNName").val('');
